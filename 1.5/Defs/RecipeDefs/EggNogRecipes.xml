<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RecipeDef>
    <defName>MakeNogStarter</defName>
    <label>crush eggs to make egg nog starter</label>
    <description>Crushing eggs.</description>
    <jobString>Crushing.</jobString>
    <workAmount>200</workAmount>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
	<allowMixingIngredients>true</allowMixingIngredients>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>EggsUnfertilized</li>
			<li>EggsFertilized</li>
          </categories>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
		<categories>
            <li>EggsUnfertilized</li>
			<li>EggsFertilized</li>
		</categories>
    </fixedIngredientFilter>
    <defaultIngredientFilter>
		<categories>
            <li>EggsUnfertilized</li>
		</categories>
    </defaultIngredientFilter>
	<recipeUsers>
    		<li>Brewery</li>
			<li MayRequire="vanillaexpanded.vbrewe">VBE_Bar</li>
	</recipeUsers>
	<researchPrerequisite>nogAlchemy</researchPrerequisite>
	<products>
		<NogStarter>15</NogStarter>
	</products>
  </RecipeDef>

  <RecipeDef>
    <defName>MakeEggNog</defName>
    <label>Make egg nog.</label>
    <description>Make egg nog.</description>
    <jobString>Mixing.</jobString>
    <workAmount>400</workAmount>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <ingredients>
      <li>
        <filter>
					<thingDefs>
						<li>RawNog</li>
					</thingDefs>
        </filter>
        <count>10</count>
      </li>
	  <li>
        <filter>
			<thingDefs>
				<li>Milk</li>
				<li>ST_Green_Milk</li>
				<li>ST_GnomeSugar</li>
				<li MayRequire="sarg.alphaanimals">AA_BlueMilk</li>
				<li MayRequire="sarg.alphaanimals">AA_GowMilk</li>
				<li MayRequire="OskarPotocki.VFE.Insectoid">VFEI_InsectMilk</li>
				<li>BORPA_Milk</li>
				<li>Rylex_CoconutMilk</li>
			</thingDefs>
        </filter>
        <count>10</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
			<thingDefs>
				<li>Milk</li>
				<li>ST_Green_Milk</li>
				<li>ST_GnomeSugar</li>
				<li MayRequire="sarg.alphaanimals">AA_BlueMilk</li>
				<li MayRequire="sarg.alphaanimals">AA_GowMilk</li>
				<li MayRequire="OskarPotocki.VFE.Insectoid">VFEI_InsectMilk</li>
				<li>Rylex_CoconutMilk</li>
				<li>RawNog</li>
				<li>BORPA_Milk</li>
			</thingDefs>
    </fixedIngredientFilter>
    <defaultIngredientFilter>
					<thingDefs>
						<li>RawNog</li>
						<li>Milk</li>
						<li>ST_Green_Milk</li>
						<li>BORPA_Milk</li>
					</thingDefs>
    </defaultIngredientFilter>
	<recipeUsers>
			<li>Brewery</li>
	</recipeUsers>
	<researchPrerequisite>nogAlchemy</researchPrerequisite>
	<products>
		<ST_EggNog>10</ST_EggNog>
	</products>
  </RecipeDef>
  
