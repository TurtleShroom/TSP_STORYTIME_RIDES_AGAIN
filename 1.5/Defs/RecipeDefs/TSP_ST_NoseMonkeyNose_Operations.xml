  <RecipeDef ParentName="SurgeryInstallBodyPartNaturalBase">
    <defName>TSP_ST_NoseMonkey_Rebirth</defName>
    <label>repair grand nose monkey nose</label>
    <description>No grand nose monkey is relevant without its bulbous nose. This surgery reattaches a displaced or destroyed grand nose monkey's massive nose. As Big Nose Ted intended.</description>
    <descriptionHyperlinks>
		<ThingDef>TSP_ST_NoseMonkeyNose</ThingDef>
		<RecipeDef>InstallNaturalHeart</RecipeDef>
	</descriptionHyperlinks>
    <jobString>emulating Big Nose Ted</jobString>
    <deathOnFailedSurgeryChance>0.05</deathOnFailedSurgeryChance>
    <skillRequirements>
      <Medicine>1</Medicine>
    </skillRequirements>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>TSP_ST_NoseMonkeyNose</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>TSP_ST_NoseMonkeyNose</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>TSP_ST_BigNoseTed_NoseEquivalent</li>
    </appliedOnFixedBodyParts>
  </RecipeDef>
