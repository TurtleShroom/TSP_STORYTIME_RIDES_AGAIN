<Defs>
	<RecipeDef>
		<defName>TSP_ST_AlloyBlackSteel</defName>
		<label>refine Black Steel</label>
		<description>Refine Black Steel from the ground mixture of iron and Blackium.</description>
		<jobString>Smelting alloy mixture to Black Steel.</jobString>
		<workAmount>1600</workAmount>
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<unfinishedThingDef>TSP_ST_Unfinished</unfinishedThingDef>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>ST_BlackMixture</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<products>
			<ST_BlackSteel>15</ST_BlackSteel>
		</products>
		<fixedIngredientFilter>
			<thingDefs>
				<li>ST_BlackMixture</li>
			</thingDefs>
		</fixedIngredientFilter>
		<recipeUsers>
			<li>BlastFurnaceHeater</li>
		</recipeUsers>
		<researchPrerequisite>alloyCrafting</researchPrerequisite>
	</RecipeDef>
	
