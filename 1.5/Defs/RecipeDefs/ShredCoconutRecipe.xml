<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RecipeDef>
    <defName>Make_ShreddedCoconut</defName>
    <label>shred coconuts</label>
    <description>Grind the flesh of coconuts into delicious shavings that go great on any seafood.</description>
    <jobString>shredding coconuts</jobString>
    <workAmount>300</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <workSkill>Cooking</workSkill>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <ingredients>
      <li>
        <filter>
			<customSummary>coconuts</customSummary>
			<thingDefs>
				<li>Rylex_Coconut</li>
			</thingDefs>
        </filter>
        <count>25</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
		<thingDefs>
			<li>Rylex_Coconut</li>
		</thingDefs>
    </fixedIngredientFilter>
    <defaultIngredientFilter>
		<thingDefs>
			<li>Rylex_Coconut</li>
		</thingDefs>
    </defaultIngredientFilter>
	<recipeUsers>
		<li>ElectricGrinder</li>
		<li>ManualGrinder</li>
		<li MayRequire="vanillaexpanded.vcooke">VCE_CondimentPrepTable</li>
	</recipeUsers>
	<products>
		<ST_ShreddedCoconut>75</ST_ShreddedCoconut>
		<Rylex_CoconutMilk>5</Rylex_CoconutMilk>
	</products>
	<researchPrerequisites>
		<li MayRequire="vanillaexpanded.vcooke">VCE_CondimentsResearch</li>
	</researchPrerequisites>
  </RecipeDef>
  
