  <AlienRace.AlienBackstoryDef ParentName="JeubBackstories">
    <defName>Farmer_Jeub</defName>
    <title>Botox Farmer</title>
	<titleShort>Botox junkie</titleShort>
    <baseDesc>[PAWN_nameDef] worked on a farm that cultivated samples of botulinium toxin bacteria, from recreational Botox was processed. [PAWN_nameDef] was known to consume a great deal of the farm's supply.</baseDesc>
    <slot>Adulthood</slot>
    <skillGains>
      <Intellectual>2</Intellectual>
	  <Plants>6</Plants>
    </skillGains>
	<forcedTraitsChance>
		<li>
			<options>
				<li>
					<defName Degree="2">DrugDesire</defName>
				</li>
				<li>
					<defName Degree="1">DrugDesire</defName>
				</li>
			</options>
			<chance>999</chance>
		</li>
    </forcedTraitsChance>
    <forcedHediffs>
      <li>BotoxAddiction</li>
    </forcedHediffs>
	<forcedItems>
		<BotoxPowder>18~36</BotoxPowder>
	</forcedItems>
	<requiredWorkTags>
