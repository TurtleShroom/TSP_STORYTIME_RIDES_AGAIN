  <AlienRace.AlienBackstoryDef ParentName="JeubBackstories">
    <defName>Pacifist_Jeub</defName>
    <title>Jeub Pacifist</title>
	<titleShort>pacifist</titleShort>
    <baseDesc>[PAWN_nameDef] saw the horror and violence of [PAWN_possessive] Jeub hive and desired to change it. Unfortunately, [PAWN_pronoun] quickly realized that most of the High Jeubs of [PAWN_possessive] hive were inherently violent; the peaceful ambition of [PAWN_nameDef] was pointless.</baseDesc>
    <slot>Adulthood</slot>
    <skillGains>
      <Social>6</Social>
	  <Intellectual>4</Intellectual>
    </skillGains>
	<forcedTraitsChance>
		<!--
		<li>
			<options>
				<li>
					<defName Degree="-2">NaturalMood</defName>
				</li>
				<chance>0</chance>
			</options>
		</li>
		-->
		<li>
			<options>
				<li>
					<defName>Kind</defName>
				</li>
			</options>
			<chance>999</chance>
		</li>
    </forcedTraitsChance>
