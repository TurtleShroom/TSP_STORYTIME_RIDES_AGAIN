  <AlienRace.AlienBackstoryDef ParentName="STMedievalBackstories">
    <defName>ST_ChildCrusader</defName>
    <title>popular crusader</title>
	<titleShort>crusader</titleShort>
    <description>[PAWN_nameDef] took part in a makeshift "popular crusade", unsanctioned by [PAWN_possessive] people's faith, and led by children and peasants to try and drive out a religious enemy. It was an absolute disaster and many died.</description>
    <slot>Childhood</slot>
    <skillGains>
      <Melee>1</Melee>
    </skillGains>
    <forcedTraitsChance>
        <li>
			<options>
				<li>
					<defName>Ascetic</defName>
				</li>
			</options>
            <chance>90</chance>
        </li>
    </forcedTraitsChance>
	<relationSettings>
		<relationChanceModifierSibling>0</relationChanceModifierSibling>
	</relationSettings>
