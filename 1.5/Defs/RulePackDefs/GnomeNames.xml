<?xml version="1.0" encoding="utf-8" ?>

<Defs>
	<RulePackDef>
		<defName>GnomeMaleName</defName>
		<rulePack>
			<rulesStrings>
				<li>name->[GnomeMaleList] [GnomeFamilyList]</li>
			</rulesStrings>
			<rulesRaw>
				<li Class="Rule_File">
					<keyword>GnomeMaleList</keyword>
					<path>GnomeNames/GnomeMale</path>
				</li>
				<li Class="Rule_File">
					<keyword>GnomeFamilyList</keyword>
					<path>GnomeNames/GnomeLastNames</path>
				</li>
			</rulesRaw>
		</rulePack>
	</RulePackDef>

	<RulePackDef>
		<defName>GnomeFemaleName</defName>
		<rulePack>
			<rulesStrings>
				<li>name->[GnomeFemaleList] [GnomeFamilyList]</li>
			</rulesStrings>
			<rulesRaw>
				<li Class="Rule_File">
					<keyword>GnomeFemaleList</keyword>
					<path>GnomeNames/GnomeFemale</path>
				</li>
				<li Class="Rule_File">
					<keyword>GnomeFamilyList</keyword>
					<path>GnomeNames/GnomeLastNames</path>
				</li>
			</rulesRaw>
		</rulePack>
	</RulePackDef>
	
	<RulePackDef>
		<defName>GnomeSettlementName</defName>
		<rulePack>
			<rulesStrings>
				<li>name-&gt;[GnomePack]</li>
			</rulesStrings>
			<rulesRaw>
				<li Class="Rule_File">
					<keyword>GnomePack</keyword>
					<path>GnomeNames/GnomeSettlement</path>
				</li>
			</rulesRaw>
		</rulePack>
	</RulePackDef>
</Defs>