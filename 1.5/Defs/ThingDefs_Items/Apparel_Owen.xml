<?xml version="1.0" encoding="utf-8" ?>

<Defs>
	<ThingDef ParentName="ApparelBase">
		<defName>Apparel_Owenhelmet</defName>
		<label>Owen Wilson mask</label>
		<description>A glorious mask to show dedication towards the Cult of Owen Wilson. Wow.</description>
		<techLevel>Ultra</techLevel>
		<graphicData>
			<texPath>Things/Apparel/OwenWilson/owenmask</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<!-- SHUT MY MOUTH AND SLAP MY GRANDMA! IT NOW ACCEPTS STUFF!! -->
		<costStuffCount>420</costStuffCount>
		<stuffCategories>
			<li>Metallic</li>

			<!-- Support for Mod Stuff -->
			<li MayRequire="Dubwise.Rimefeller">Plastic</li>
			<li MayRequire="Dubwise.Rimefeller">Composite</li>
			<li MayRequire="sarg.alphabiomes">AB_ExoticMetals</li>
		</stuffCategories>
		<statBases>
			<WorkToMake>40000</WorkToMake>
			<MaxHitPoints>160</MaxHitPoints>
			<MarketValue>420</MarketValue>
			<Mass>2.7</Mass>
			<ArmorRating_Blunt>0.8</ArmorRating_Blunt>
			<ArmorRating_Sharp>0.85</ArmorRating_Sharp>
			<ArmorRating_Heat>0</ArmorRating_Heat>
			<Flammability>10</Flammability>
			<Insulation_Cold>29</Insulation_Cold>
			<Insulation_Heat>20</Insulation_Heat>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>-0.5</MoveSpeed>
			<PsychicSensitivity>2</PsychicSensitivity>
			<SocialImpact>1.5</SocialImpact>
		</equippedStatOffsets>
		<apparel>
			<developmentalStageFilter>Child, Adult</developmentalStageFilter>
			<bodyPartGroups>
				<li>FullHead</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Apparel/OwenWilson/owenmask</wornGraphicPath>
			<layers>
				<li>Overhead</li>
			</layers>
			<!--  <commonality>2</commonality> -->
			<tags>
				<li>Owenapparel</li>
			</tags>
			<defaultOutfitTags>
				<li>Owenapparel</li>
			</defaultOutfitTags>
			<hatRenderedFrontOfFace>true</hatRenderedFrontOfFace>
			<canBeGeneratedToSatisfyWarmth>false</canBeGeneratedToSatisfyWarmth>
			<countsAsClothingForNudity>false</countsAsClothingForNudity>
		</apparel>
		<comps>
			<li Class="CompProperties_Biocodable"></li>
		</comps>
	</ThingDef>

