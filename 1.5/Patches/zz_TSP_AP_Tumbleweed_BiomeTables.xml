<?xml version="1.0" encoding="utf-8"?>

<Patch>
	<Operation Class="PatchOperationConditional">
	<!-- Does this exist? -->
	<xpath>/Defs/ThingDef[defName="AP_TumbleWeed_Race"]</xpath>
		<!-- If it does, do this replacement. -->
		<match Class="PatchOperationReplace">
			<success>Always</success>
			<xpath>/Defs/ThingDef[defName="AP_TumbleWeed_Race"]/race/wildBiomes</xpath>
			<value>
				<wildBiomes Inherit="False">
					<IceSheet>0.00</IceSheet>
					<SeaIce>0.00</SeaIce>
					<ColdBog>0.00</ColdBog>
					<Desert>15</Desert>
					<BorealForest>0.05</BorealForest>
					<Tundra>0.08</Tundra>
					<TemperateForest>0.05</TemperateForest>
					<TemperateSwamp>0.1</TemperateSwamp>
					<TropicalRainforest>0.008</TropicalRainforest>
					<TropicalSwamp>0.03</TropicalSwamp>
					<AridShrubland>9</AridShrubland>
					<ExtremeDesert>1</ExtremeDesert>
						
						
						
						
					<!-- ___________________ -->			
					<!-- Mod Biomes -->
					<!-- ___________________ -->

					<!-- sarg.alphabiomes -->
					<!-- ALPHA BIOMES -->

					<AB_FeraliskInfestedJungle MayRequire="sarg.alphabiomes">0.001</AB_FeraliskInfestedJungle>

					<AB_GallatrossGraveyard MayRequire="sarg.alphabiomes">7</AB_GallatrossGraveyard>

					<AB_GelatinousSuperorganism MayRequire="sarg.alphabiomes">1</AB_GelatinousSuperorganism>

					<AB_MechanoidIntrusion MayRequire="sarg.alphabiomes">10</AB_MechanoidIntrusion>

					<AB_MycoticJungle MayRequire="sarg.alphabiomes">0.004</AB_MycoticJungle>

					<AB_OcularForest MayRequire="sarg.alphabiomes">0.4</AB_OcularForest>

					<AB_PropaneLakes MayRequire="sarg.alphabiomes">0</AB_PropaneLakes>

					<AB_PyroclasticConflagration MayRequire="sarg.alphabiomes">0</AB_PyroclasticConflagration>

					<AB_RockyCrags MayRequire="sarg.alphabiomes">1</AB_RockyCrags>

					<AB_TarPits MayRequire="sarg.alphabiomes">2</AB_TarPits>

					<AB_MiasmicMangrove MayRequire="sarg.alphabiomes">0</AB_MiasmicMangrove>

					<AB_IdyllicMeadows MayRequire="sarg.alphabiomes">3</AB_IdyllicMeadows>



					<!-- ___________________ -->

					<!-- BiomesTeam.Oasis -->
					<!-- CHROMATIC OASIS -->

					<BMT_ChromaticOasis MayRequire="BiomesTeam.Oasis">5</BMT_ChromaticOasis>


					<!-- ___________________ -->

					<!-- Mlie.AdvancedBiomes -->
					<!-- ADVANCED BIOMES -->

					<PoisonForest MayRequire="Mlie.AdvancedBiomes">0.02</PoisonForest>

					<Savanna MayRequire="Mlie.AdvancedBiomes">9</Savanna>

					<Volcano MayRequire="Mlie.AdvancedBiomes">0.01</Volcano>

					<Wasteland MayRequire="Mlie.AdvancedBiomes">15</Wasteland>

					<Wetland MayRequire="Mlie.AdvancedBiomes">0.005</Wetland>


					<!-- ___________________ -->

					<!-- zylle.MoreVanillaBiomes -->
					<!-- More Vanilla Biomes -->

					<ZBiome_AlpineMeadow MayRequire="zylle.MoreVanillaBiomes">0.8</ZBiome_AlpineMeadow>

					<ZBiome_CloudForest MayRequire="zylle.MoreVanillaBiomes">0.001</ZBiome_CloudForest>

					<ZBiome_CoastalDunes MayRequire="zylle.MoreVanillaBiomes">5</ZBiome_CoastalDunes>

					<ZBiome_DesertOasis MayRequire="zylle.MoreVanillaBiomes">5</ZBiome_DesertOasis>

					<ZBiome_Grasslands MayRequire="zylle.MoreVanillaBiomes">15</ZBiome_Grasslands>

					<ZBiome_Iceberg_NoBeach MayRequire="zylle.MoreVanillaBiomes">0</ZBiome_Iceberg_NoBeach>

					<ZBiome_Marsh MayRequire="zylle.MoreVanillaBiomes">0</ZBiome_Marsh>

					<ZBiome_Sandbar_NoBeach MayRequire="zylle.MoreVanillaBiomes">10</ZBiome_Sandbar_NoBeach>
					
					<ZBiome_GlacialShield MayRequire="zylle.MoreVanillaBiomes">0.04</ZBiome_GlacialShield>
					
					<ZBiome_Badlands MayRequire="zylle.MoreVanillaBiomes">15</ZBiome_Badlands>


					<!-- ___________________ -->

					<!-- Mlie.NaturesPrettySweet -->
					<!-- NATURE IS PRETTY SWEET -->

					<TKKN_VolcanicFlow MayRequire="Mlie.NaturesPrettySweet">0</TKKN_VolcanicFlow>

					<TKKN_RedwoodForest MayRequire="Mlie.NaturesPrettySweet">0.01</TKKN_RedwoodForest>

					<TKKN_Grasslands MayRequire="Mlie.NaturesPrettySweet">15</TKKN_Grasslands> <!-- PRAIRIE -->

					<TKKN_Savanna MayRequire="Mlie.NaturesPrettySweet">15</TKKN_Savanna>

					<TKKN_Desert MayRequire="Mlie.NaturesPrettySweet">25</TKKN_Desert> <!-- SALT FIELDS -->

					<TKKN_Oasis MayRequire="Mlie.NaturesPrettySweet">5</TKKN_Oasis>			


					<!-- ___________________ -->

					<!-- Heis.Edo.Themed.Expansion -->
					<!-- HISTORICALLY ACCURATE WEEABOO MOD -->

					<HSBambooForest MayRequire="Heis.Edo.Themed.Expansion">0</HSBambooForest>


					<!-- _________________ -->

					<!-- BiomesTeam.BiomesIslands -->
					<!-- BIOMES ISLANDS -->

					<BiomesIslands_Atoll MayRequire="BiomesTeam.BiomesIslands">0</BiomesIslands_Atoll>

					<BiomesIslands_BorealIsland MayRequire="BiomesTeam.BiomesIslands">0</BiomesIslands_BorealIsland>

					<BiomesIslands_DesertIsland MayRequire="BiomesTeam.BiomesIslands">1</BiomesIslands_DesertIsland>

					<BiomesIslands_DesertArchipelago MayRequire="BiomesTeam.BiomesIslands">1</BiomesIslands_DesertArchipelago>

					<BiomesIslands_TemperateIsland MayRequire="BiomesTeam.BiomesIslands">0.05</BiomesIslands_TemperateIsland>

					<BiomesIslands_TemperateArchipelago MayRequire="BiomesTeam.BiomesIslands">0.05</BiomesIslands_TemperateArchipelago>

					<BiomesIslands_TropicalIsland MayRequire="BiomesTeam.BiomesIslands">0.005</BiomesIslands_TropicalIsland>

					<BiomesIslands_TropicalArchipelago MayRequire="BiomesTeam.BiomesIslands">0.005</BiomesIslands_TropicalArchipelago>

					<BiomesIslands_TundraIsland MayRequire="BiomesTeam.BiomesIslands">0.01</BiomesIslands_TundraIsland>


					<!-- ___________________ -->

					<!-- reel.FrozenForest -->
					<!-- WINTER TAIGA -->

					<WinterTaiga MayRequire="reel.FrozenForest">0</WinterTaiga>


					<!-- __________________ -->

					<!-- SirMashedPotato.MorrowRim -->
					<!-- "MORROWIND" MOD -->

					<MorrowRim_Ashlands MayRequire="SirMashedPotato.MorrowRim">3</MorrowRim_Ashlands>

					<MorrowRim_BlightedAshlands MayRequire="SirMashedPotato.MorrowRim">15</MorrowRim_BlightedAshlands>

					<MorrowRim_AshSwamp MayRequire="SirMashedPotato.MorrowRim.ashswamp">0.005</MorrowRim_AshSwamp>

					<MorrowRim_VolcanicAshlands MayRequire="SirMashedPotato.MorrowRim.volcanicashlands">0</MorrowRim_VolcanicAshlands>


					<!-- ___________________ -->

					<!-- pphhyy.gulden -->
					<!-- FOREST OF GOLD -->

					<TemperateGuldenForest MayRequire="pphhyy.gulden">0.005</TemperateGuldenForest>

					<LotRE_MallornForest MayRequire="lordoftherims.elves">0.005</LotRE_MallornForest>


					<!-- ___________________ -->

					<!-- deemainiac.securecontainrimworld -->
					<!-- SCP FOUNDATION -->

					<SCR_SCP610Biome MayRequire="deemainiac.securecontainrimworld">5</SCR_SCP610Biome>


					<!-- _________________ -->

					<!-- regrowth.botr.volcanicicesheet -->
					<!-- VOLCANIC ICE SHEET -->

					<RG-VIS_VulcanicIceSheet MayRequire="regrowth.botr.volcanicicesheet">0.01</RG-VIS_VulcanicIceSheet> <!-- Make me, Helixian. MAKE ME. -->
					
					
					<!-- _________________ -->
					
					<!-- ironscruff.primordialgeysers -->
					<!-- PRIMORDIAL GEYSERS -->
					

					<IronScruff_PrimordialGeysers MayRequire="ironscruff.primordialgeysers">9</IronScruff_PrimordialGeysers>
				</wildBiomes>
			</value>
		</match>
		<!-- If the first XPATH does not exist, the original code stands. -->
	</Operation>
	
