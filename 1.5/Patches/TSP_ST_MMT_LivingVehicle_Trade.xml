<Patch>
	<Operation Class="PatchOperationConditional">
	<!-- Does this exist? -->
		<xpath>/Defs/FactionDef[defName="BlueMoonCorpSW"]</xpath>
		<!-- If it does, add this. -->
		<match Class="PatchOperationAdd">
			<xpath>/Defs/FactionDef[defName="BlueMoonCorpSW"]/caravanTraderKinds</xpath>
			<success>Always</success>
			<value>
				<li MayRequire="TSP.Isengriff.Storytime">TSP_ST_MMT_Caravan_LivingVehicle</li>
			</value>
		</match>
		<!-- If the first XPATH does not exist, the original code stands. -->
    </Operation>
	
