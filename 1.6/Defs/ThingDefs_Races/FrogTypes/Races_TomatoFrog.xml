		  <MoveSpeed>4.0</MoveSpeed>
		  <MarketValue>200</MarketValue>
		  <ComfyTemperatureMin>-12</ComfyTemperatureMin>
		  <ComfyTemperatureMax>110</ComfyTemperatureMax>
		  <Wildness>0</Wildness>
		</statBases>
		<comps>
