      <MoveSpeed>12</MoveSpeed>
      <MarketValue>50</MarketValue>
      <ComfyTemperatureMin>-13</ComfyTemperatureMin>
      <ComfyTemperatureMax>37</ComfyTemperatureMax>
	  <MeatAmount>30</MeatAmount>
	  <LeatherAmount>5</LeatherAmount>
	  <Wildness>0</Wildness>
    </statBases>
