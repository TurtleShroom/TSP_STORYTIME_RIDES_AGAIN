			<max>7</max>
		</wildGroupSize>
		<!--生成されるときの最低年齢と最高年齢 -->
		<minGenerationAge>3</minGenerationAge>
		<maxGenerationAge>90</maxGenerationAge>
		<!-- 雄水晶亀 -->
