	<PawnKindDef ParentName="KomodonAnimalKindBase">
		<defName>C_turtle</defName>
		<!-- 水晶亀 -->
		<label>Crystal Turtle</label>
		<race>C_turtle</race>
		<!-- 人狩として出現するかどうか。記述無しでtrue。出現する。 -->
