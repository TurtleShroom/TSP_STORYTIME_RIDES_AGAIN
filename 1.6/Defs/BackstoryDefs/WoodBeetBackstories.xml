  <AlienRace.AlienBackstoryDef ParentName="BeetBackstories">
    <defName>CommunalChildWoodBeet</defName>
    <title>Child of the Wood Beet Commune</title>
	<titleShort>Communally Abused</titleShort>
    <description>[PAWN_nameDef] never knew [PAWN_possessive] parents or homeland. [PAWN_pronoun] was 'adopted' by Wood Beets and 'raised' communally. This involved a lot of drugs and other questionable parenting decisions.</description>
    <slot>Childhood</slot>
    <skillGains>
      <Social>3</Social>
	  <Melee>3</Melee>
	  <Shooting>3</Shooting>
    </skillGains>
	<forcedTraitsChance>
		<li>
			<options>
				<li>
					<defName Degree="2">DrugDesire</defName>
				</li>
			</options>
			<chance>100</chance>
		</li>
		<li>
			<options>
				<li>
					<defName>Psychopath</defName>
				</li>
			</options>
			<chance>5</chance>
        </li>
		<li>
			<options>
				<li>
					<defName>Cannibal</defName>
				</li>
			</options>
			<chance>1</chance>
        </li>
		<li>
			<options>
				<li>
					<defName>Bloodlust</defName>
				</li>
			</options>
			<chance>10</chance>
		</li>
    </forcedTraitsChance>
