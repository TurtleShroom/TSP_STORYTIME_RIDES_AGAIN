	<RecipeDef>
		<defName>Make_PadCandy</defName>
		<label>make D-Pad candy</label>
		<description>Grind sugar into candy and shape it like an ancient gaming console controller's Directional Pad... for some reason.</description>
		<jobString>Making.</jobString>
		<workAmount>400</workAmount>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<unfinishedThingDef>TSP_ST_Unfinished</unfinishedThingDef>
		<ingredients>
		  <li>
			<filter>
				<customSummary>sugar, sweeteners</customSummary>
				<thingDefs>
					<li>ST_GnomeSugar</li>
					<li MayRequire="vanillaexpanded.vcooke">VCE_RawSugar</li>
					<li MayRequire="aratocat.wonderland">Chocofuel</li>
				</thingDefs>
				<disallowedThingDefs>
					<li>Chocolate</li>
				</disallowedThingDefs>
			</filter>
			<count>15</count>
		  </li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>ST_GnomeSugar</li>
				<li MayRequire="vanillaexpanded.vcooke">VCE_RawSugar</li>
				<li MayRequire="aratocat.wonderland">Chocofuel</li>
			</thingDefs>
			<disallowedThingDefs>
				<li>Chocolate</li>
			</disallowedThingDefs>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<thingDefs>
				<li>ST_GnomeSugar</li>
				<li MayRequire="vanillaexpanded.vcooke">VCE_RawSugar</li>
			</thingDefs>
			<disallowedThingDefs>
				<li MayRequire="vanillaexpanded.vcooke">VCE_RawSugar</li>
			</disallowedThingDefs>
		</defaultIngredientFilter>
		<recipeUsers>
				<li>ElectricStove</li>
				<li>FueledStove</li>
				<li>GnomeTable</li>
				<li>ManualGrinder</li>
				<li>ElectricGrinder</li>
		</recipeUsers>
		<researchPrerequisites>
			<li>advancedCooking</li>
		</researchPrerequisites>
		<products>
			<PadCandy>3</PadCandy>
		</products>
	</RecipeDef>

	<RecipeDef>
		<defName>Make_Fuh</defName>
		<label>make Fuh</label>
		<description>Mix cheese, Gnome poppy fibers, and either meat or vegetables into an ornamental, aesthetically pleasing dish.</description>
		<jobString>Mixing Fuh</jobString>
		<workAmount>500</workAmount>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<unfinishedThingDef>TSP_ST_Unfinished</unfinishedThingDef>
		<ingredients>
		  <li>
			<filter>
				<customSummary>meat, vegetarian</customSummary>
			  <categories>
				<li>MeatRaw</li>
				<li MayRequire="deemainiac.securecontainrimworld">SCPMeat</li>
				<li>PlantFoodRaw</li>
			  </categories>
			</filter>
			<count>10</count>
		  </li>
		  <li>
			<filter>
				<customSummary>cheese</customSummary>
				<categories>
					<li MayRequire="vanillaexpanded.vcooke">VCE_Cheese</li>
				</categories>
				<thingDefs>
					<li MayRequire="TSP.Isengriff.Storytime">ST_ProcessedCheese</li>
				</thingDefs>
			</filter>
			<count>5</count>
		  </li>
		  <li>
			<filter>
				<customSummary>Gnome poppy fibers</customSummary>
			  <thingDefs>
				<li>Gnome_Cloth</li>
			  </thingDefs>
			</filter>
			<count>5</count>
		  </li>
		</ingredients>
		<fixedIngredientFilter>
		  <categories>
			<li>MeatRaw</li>
			<li MayRequire="deemainiac.securecontainrimworld">SCPMeat</li>
			<li>PlantFoodRaw</li>
			<li MayRequire="vanillaexpanded.vcooke">VCE_Cheese</li>
		  </categories>
		  <thingDefs>
			<li MayRequire="TSP.Isengriff.Storytime">ST_ProcessedCheese</li>
		  </thingDefs>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
		  <categories>
			<li>MeatRaw</li>
		  </categories>
		  <thingDefs>
			<li MayRequire="TSP.Isengriff.Storytime">ST_Bean</li>
			<li MayRequire="vvenchov.vvnewharvestgardencrops">VV_BlackBeans</li>
			<li MayRequire="zal.braziliancrops">KIT_Beans</li>
			<li MayRequire="dankpyon.medieval.overhaul">DankPyon_RawLentils</li>
			<li MayRequire="TSP.Isengriff.Storytime">ST_ProcessedCheese</li>
			<li MayRequire="mlie.tropickapprainforestmod">CK_RawJitterBerries</li>
			<li>Gnome_Cloth</li>
		  </thingDefs>
		</defaultIngredientFilter>
		<recipeUsers>
				<li>ElectricStove</li>
				<li>FueledStove</li>
				<li>GnomeTable</li>
		</recipeUsers>
		<researchPrerequisites>
			<li>advancedCooking</li>
		</researchPrerequisites>
		<products>
			<Fuh>8</Fuh>
		</products>
	</RecipeDef>
	  
	<RecipeDef>
		<defName>Make_Burrito</defName>
		<label>Make Meaty Five Layer Burritos</label>
		<description>Make Meaty Five Layer Burritos.</description>
		<jobString>imitating Hispanic cuisine</jobString>
		<workAmount>400</workAmount>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<unfinishedThingDef>TSP_ST_Unfinished</unfinishedThingDef>
		<ingredients>
		  <li>
			<filter>
						<customSummary>beans</customSummary>
						<thingDefs>
							<li MayRequire="TSP.Isengriff.Storytime">ST_Bean</li>
							<li MayRequire="vvenchov.vvnewharvestgardencrops">VV_BlackBeans</li>
							<li MayRequire="zal.braziliancrops">KIT_Beans</li>
							<li MayRequire="dankpyon.medieval.overhaul">DankPyon_RawLentils</li>
							<li MayRequire="mlie.tropickapprainforestmod">CK_RawJitterBerries</li>
						</thingDefs>
			</filter>
			<count>25</count>
		  </li>
		  <li>
			<filter>
						<customSummary>cheeses</customSummary>
						<categories>
							<li MayRequire="vanillaexpanded.vcooke">VCE_Cheese</li>
							<li MayRequire="dankpyon.medieval.overhaul">DankPyon_Cheese</li>
						</categories>
						<thingDefs>
							<li MayRequire="TSP.Isengriff.Storytime">ST_ProcessedCheese</li>
						</thingDefs>
			</filter>
			<count>5</count>
		  </li>
		  <li>
			<filter>
				<customSummary>meat</customSummary>
			  <categories>
				<li>MeatRaw</li>
				<li MayRequire="deemainiac.securecontainrimworld">SCPMeat</li>
			  </categories>
			</filter>
			<count>10</count>
		  </li>
		  <li>
			<filter>
				<customSummary>vegetarian</customSummary>
			  <categories>
				<li>PlantFoodRaw</li>
			  </categories>
			  <thingDefs>
				<li MayRequire="aratocat.wonderland">Chocofuel</li> <!-- Don't ask -->
			  </thingDefs>
			  <disallowedThingDefs>
				<li MayRequire="TSP.Isengriff.Storytime">ST_Bean</li>
				<li MayRequire="vvenchov.vvnewharvestgardencrops">VV_BlackBeans</li>
				<li MayRequire="zal.braziliancrops">KIT_Beans</li>
				<li MayRequire="dankpyon.medieval.overhaul">DankPyon_RawLentils</li>
				<li MayRequire="mlie.tropickapprainforestmod">CK_RawJitterBerries</li>
			  </disallowedThingDefs>
			</filter>
			<count>10</count>
		  </li>
		</ingredients>
		<fixedIngredientFilter>
						<thingDefs>
							<li MayRequire="TSP.Isengriff.Storytime">ST_Bean</li>
							<li MayRequire="mlie.tropickapprainforestmod">CK_RawJitterBerries</li>
							<li MayRequire="TSP.Isengriff.Storytime">ST_ProcessedCheese</li>
							<li MayRequire="aratocat.wonderland">Chocofuel</li>
						</thingDefs>
						<categories>
							<li>MeatRaw</li>
							<li MayRequire="deemainiac.securecontainrimworld">SCPMeat</li>
							<li>PlantFoodRaw</li>
							<li MayRequire="vanillaexpanded.vcooke">VCE_Cheese</li>
						</categories>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
						<categories>
							<li>MeatRaw</li>
							<li MayRequire="deemainiac.securecontainrimworld">SCPMeat</li>
							<li>PlantFoodRaw</li>
							<!-- <li MayRequire="vanillaexpanded.vcooke">VCE_Cheese</li> -->
						</categories>
						<thingDefs>
							<li MayRequire="TSP.Isengriff.Storytime">ST_Bean</li>
							<li MayRequire="TSP.Isengriff.Storytime">ST_ProcessedCheese</li>
						</thingDefs>
		</defaultIngredientFilter>
		<recipeUsers>
				<li>ElectricStove</li>
				<li>FueledStove</li>
				<li>GnomeTable</li>
		</recipeUsers>
		<researchPrerequisites>
			<li>advancedCooking</li>
		</researchPrerequisites>
		<products>
			<TacoBellBurrito>2</TacoBellBurrito>
		</products>
	</RecipeDef>
</Defs>