<?xml version="1.0" encoding="utf-8" ?>

<Defs>
	<RecipeDef>
		<defName>Make_MeatNugget</defName>
		<label>make meat nugget</label>
		<description>Condense meat into a highly nutritious nugget.</description>
		<jobString>Condensing</jobString>
		<workAmount>70</workAmount>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<unfinishedThingDef>TSP_ST_Unfinished</unfinishedThingDef>
		<ingredients>
		  <li>
			<filter>
				<customSummary>meats</customSummary>
			  <categories>
				<li>MeatRaw</li>
				<li MayRequire="deemainiac.securecontainrimworld">SCPMeat</li>
			  </categories>
			</filter>
			<count>18</count>
		  </li>
		</ingredients>
		<fixedIngredientFilter>
		  <categories>
			<li>MeatRaw</li>
			<li MayRequire="deemainiac.securecontainrimworld">SCPMeat</li>
		  </categories>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
		  <categories>
			<li>MeatRaw</li>
			<li MayRequire="deemainiac.securecontainrimworld">SCPMeat</li>
		  </categories>
		</defaultIngredientFilter>
		<recipeUsers>
				<li>ElectricStove</li>
				<li>FueledStove</li>
				<li>Campfire</li>
				<li>GnomeTable</li>
		</recipeUsers>
		<products>
			<MeatNugget>4</MeatNugget>
		</products>
	</RecipeDef>
	
