<?xml version="1.0" encoding="utf-8" ?>
<Defs>

 <RecipeDef>
    <defName>MakeBlackMix</defName>
    <label>Make Black Mixture</label>
    <description>Grind Blackium and iron into a fine mixture.</description>    
	<jobString>Grind Blackium and iron into a fine mixture.</jobString>
    <workAmount>600</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>CutStone</effectWorking>
    <soundWorking>Interact_Drill</soundWorking>
	<allowMixingIngredients>true</allowMixingIngredients>
	<unfinishedThingDef>TSP_ST_Unfinished</unfinishedThingDef>
	<recipeUsers>
      <li>ManualGrinder</li>
      <li>ElectricGrinder</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
					<customSummary>blackium</customSummary>
					<thingDefs>
						<li>ST_Blackium</li>
					</thingDefs>
        </filter>
        <count>10</count>
      </li>
	  <li>
        <filter>
					<customSummary>iron</customSummary>
					<thingDefs>
						<li>Steel</li>
						<li MayRequire="Mlie.AncientRimCore">AR_Iron</li>
					</thingDefs>
        </filter>
        <count>15</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
					<thingDefs>
						<li>ST_Blackium</li>
						<li>Steel</li>
						<li MayRequire="Mlie.AncientRimCore">AR_Iron</li>
					</thingDefs>
    </fixedIngredientFilter>
	<products>
		<ST_BlackMixture>30</ST_BlackMixture>
	</products>
	<researchPrerequisites>
		<li>alloyCrafting</li>
	</researchPrerequisites>
  </RecipeDef>

