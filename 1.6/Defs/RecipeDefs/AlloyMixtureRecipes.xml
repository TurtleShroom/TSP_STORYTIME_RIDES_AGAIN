 <RecipeDef>
    <defName>MakeAdamantMix</defName>
    <label>Make Mordor Mixture</label>
    <description>Grind Mordor Bricks, jade, and either iron or tin into a fine mixture.</description>    
	<jobString>Grinding ingredients into Mordor Mixtures.</jobString>
    <workAmount>900</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>CutStone</effectWorking>
    <soundWorking>Interact_Drill</soundWorking>
	<allowMixingIngredients>true</allowMixingIngredients>
	<unfinishedThingDef>TSP_ST_Unfinished</unfinishedThingDef>
	<recipeUsers>
      <li>ManualGrinder</li>
      <li>ElectricGrinder</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
					<customSummary>Mordor Bricks</customSummary>
					<thingDefs>
						<li>Mordor_Brick</li>
					</thingDefs>
        </filter>
        <count>20</count>
      </li>
	  <li>
        <filter>
					<customSummary>iron, tin</customSummary>
					<thingDefs>
						<li>Steel</li>
						<li MayRequire="TSP.Isengriff.BronzeCopperTin.Split">ST_Tin</li>
						<li MayRequire="Mlie.AncientRimCore">AR_Iron</li>
					</thingDefs>
        </filter>
        <count>10</count>
      </li>
	  <li>
        <filter>
					<customSummary>jade</customSummary>
					<thingDefs>
						<li>Jade</li>
					</thingDefs>
        </filter>
        <count>3</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
					<thingDefs>
						<li>Mordor_Brick</li>
						<li MayRequire="TSP.Isengriff.BronzeCopperTin.Split">ST_Tin</li>
						<li MayRequire="Mlie.AncientRimCore">AR_Iron</li>
						<li>Steel</li>
						<li>Jade</li>
					</thingDefs>
    </fixedIngredientFilter>
	<products>
		<ST_AdamantiumMixture>40</ST_AdamantiumMixture>
	</products>
	<researchPrerequisites>
		<li>alloyCrafting</li>
	</researchPrerequisites>
  </RecipeDef>

 <RecipeDef>
    <defName>MakeRuniteMix</defName>
    <label>Make Runite Mixture</label>
    <description>Grind stones, gold, iron, and Mordor Bricks into a fine mixture.</description>    
	<jobString>Grind arcaellum and steel into a fine mixture.</jobString>
    <workAmount>900</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>CutStone</effectWorking>
    <soundWorking>Interact_Drill</soundWorking>
	<allowMixingIngredients>true</allowMixingIngredients>
	<unfinishedThingDef>TSP_ST_Unfinished</unfinishedThingDef>
	<recipeUsers>
      <li>ManualGrinder</li>
      <li>ElectricGrinder</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
			<customSummary>Mordor Bricks, arcalleum</customSummary>
			<thingDefs>
				<li MayRequire="Torann.ARimworldOfMagic">TM_Arcalleum</li>
				<li>Mordor_Brick</li>
			</thingDefs>
        </filter>
        <count>10</count>
      </li>
	  <li>
        <filter>
			<customSummary>iron</customSummary>
			<thingDefs>
				<li>Steel</li>
				<li MayRequire="Mlie.AncientRimCore">AR_Iron</li>
			</thingDefs>
        </filter>
        <count>20</count>
      </li>
	  <li>
		<filter>
			<customSummary>gold</customSummary>
			<thingDefs>
				<li>Gold</li>
			</thingDefs>
		</filter>
		<count>5</count>
	  </li>
	  <li>
			<filter>
				<customSummary>stone bricks</customSummary>
				<categories>
					<li>StoneBlocks</li>
				</categories>
			</filter>
			<count>40</count>
	  </li>
    </ingredients>
    <fixedIngredientFilter>
		<categories>
			<li>StoneBlocks</li>
		</categories>
		<thingDefs>
			<li MayRequire="Torann.ARimworldOfMagic">TM_Arcalleum</li>
			<li>Mordor_Brick</li>
			<li>Gold</li>
			<li>Steel</li>
			<li MayRequire="Mlie.AncientRimCore">AR_Iron</li>
		</thingDefs>
    </fixedIngredientFilter>
	<products>
		<ST_RuniteMixture>60</ST_RuniteMixture>
	</products>
	<researchPrerequisites>
		<li>alloyCrafting</li>
	</researchPrerequisites>
  </RecipeDef>
  
