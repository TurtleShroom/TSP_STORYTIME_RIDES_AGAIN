<Defs>

	<!-- FROM GNOME FLOUR TO GNOME MEAL -->

  <RecipeDef>
    <defName>TSP_ST_Make_GnomeFlour2_XV</defName>
    <label>Designate gnome flour for general use (X25)</label>
    <description>Use this "Recipe" to add gnome flour into the general plant-based ingredients (in that designation, it is called "gnome meal").</description>
	<descriptionHyperlinks>
		<ThingDef>GnomeFlour</ThingDef>
		<ThingDef>GnomeFlourMeal</ThingDef>
	</descriptionHyperlinks>
    <jobString>Assigning gnome meal</jobString>
    <workAmount>0</workAmount>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>PlayPoker</effectWorking>
    <soundWorking>Power_OnSmall</soundWorking>
    <ingredients>
      <li>
        <filter>
					<thingDefs>
						<li>GnomeFlour</li>
					</thingDefs>
        </filter>
        <count>25</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
					<thingDefs>
						<li>GnomeFlour</li>
					</thingDefs>
    </fixedIngredientFilter>
    <defaultIngredientFilter>
					<thingDefs>
						<li>GnomeFlour</li>
					</thingDefs>
    </defaultIngredientFilter>
	<recipeUsers>
    		<li>GnomeTable</li>
			<li>ElectricStove</li>
			<li>FueledStove</li>
			<li>ManualGrinder</li>
			<li>ElectricGrinder</li>
			<li MayRequire="Dajian.ChiTeaditional.Expanded">Chi_RiceMaker_Old</li>
			<li MayRequire="Dajian.ChiTeaditional.Expanded">Chi_RiceMaker</li>
			<li MayRequire="mlie.takisrandomstuff">ButcherSpot</li>
	</recipeUsers>
	<researchPrerequisite>ST_GnomeCulture</researchPrerequisite>
	<products>
		<GnomeFlourMeal>25</GnomeFlourMeal>
	</products>
  </RecipeDef>
  
