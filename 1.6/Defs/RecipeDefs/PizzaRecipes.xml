<?xml version="1.0" encoding="utf-8" ?>

<Defs>
  <RecipeDef>
    <defName>Make_CheesePizza</defName>
    <label>Make a cheese pizza</label>
    <description>Make a cheese pizza. I better not show you where the crust is made.</description>
    <jobString>Making.</jobString>
    <workAmount>600</workAmount>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <ingredients>
      <li>
        <filter>
			<customSummary>cheese</customSummary>
			<categories>
				<li MayRequire="vanillaexpanded.vcooke">VCE_Cheese</li>
			</categories>
			<thingDefs>
				<li MayRequire="TSP.Isengriff.Storytime">ST_ProcessedCheese</li>
			</thingDefs>
        </filter>
        <count>10</count>
      </li>
	  <li>
        <filter>
					<customSummary>tomatoes</customSummary>
					<thingDefs>
						<li>TomatoCyst</li>
						<li MayRequire="VanillaExpanded.VPlantsE">VCE_RawTomatoes</li>
						<li MayRequire="onekimchiman.ingredienttime">O_TomatoSauce</li>
					</thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
					<thingDefs>
						<li>TomatoCyst</li>
						<li MayRequire="VanillaExpanded.VPlantsE">VCE_RawTomatoes</li>
						<li MayRequire="TSP.Isengriff.Storytime">ST_ProcessedCheese</li>
						<li MayRequire="onekimchiman.ingredienttime">O_TomatoSauce</li>
					</thingDefs>
					<categories>
						<li MayRequire="vanillaexpanded.vcooke">VCE_Cheese</li>
					</categories>
    </fixedIngredientFilter>
    <defaultIngredientFilter>
					<thingDefs>
						<li>TomatoCyst</li>
						<li MayRequire="TSP.Isengriff.Storytime">ST_ProcessedCheese</li>
					</thingDefs>
    </defaultIngredientFilter>
	<recipeUsers>
			<li>ElectricStove</li>
			<li>FueledStove</li>
			<li MayRequire="morishep.tarojun.onioftherim">OotR_GroundOven</li>
			<li>GnomeTable</li>
	</recipeUsers>
	<researchPrerequisite>pizzaTime</researchPrerequisite>
	<products>
		<ST_CheesePizza>6</ST_CheesePizza>
	</products>
  </RecipeDef>
  
