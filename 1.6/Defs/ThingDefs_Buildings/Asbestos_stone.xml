    <RecipeDef>
        <defName>Asbestos_Make_Tile</defName>
        <label>make asbestos tiles</label>
        <description>Make asbestos tiles.</description>
        <jobString>Making asbestos tiles.</jobString>
        <effectWorking>Smelt</effectWorking>
        <soundWorking>Recipe_Smelt</soundWorking>
        <workAmount>600</workAmount>
        <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
        <ingredients>
            <li>
                <filter>
					<customSummary>asbestos</customSummary>
                    <thingDefs>
                        <li>Asbestos_Fiber</li>
                    </thingDefs>
					<disallowedCategories>
						<li>StoneBlocks</li>
						<li>ST_AlloyMetals</li>
					</disallowedCategories>
					<disallowedThingDefs>
						<li>Asbestos_Chunk</li>
						<li>Asbestos_Tile</li>
					</disallowedThingDefs>
                </filter>
                <count>50</count>
            </li>
            <li>
                <filter>
					<customSummary>stone blocks, steel, iron, bronze</customSummary>
					<categories>
						<li>StoneBlocks</li>
						<li>ST_AlloyMetals</li>
					</categories>
                    <thingDefs>
                        <li>Steel</li>
						<li MayRequire="mlie.ancientrimcore">AR_Iron</li>
						<li MayRequire="mlie.ancientrimcore">AR_Copper</li>
						<li MayRequire="mlie.ancientrimcore">AR_Bronze</li>
						<li MayRequire="tsp.isengriff.bronzecoppertin.split">ST_Copper</li>
						<li MayRequire="tsp.isengriff.bronzecoppertin.split">ST_Tin</li>
						<li MayRequire="tsp.isengriff.bronzecoppertin.split">ST_Bronze</li>
						<li MayRequire="vanillaexpanded.recycling">VRecyclingE_TrashBrick</li>
                    </thingDefs>
					<disallowedThingDefs>
						<li>Asbestos_Tile</li>
						<li>Asbestos_Chunk</li>
					</disallowedThingDefs>
                </filter>
                <count>10</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
			<categories>
				<li>StoneBlocks</li>
				<li>ST_AlloyMetals</li>
			</categories>
            <thingDefs>
                <li>Asbestos_Fiber</li>
                <li>Steel</li>
				<li MayRequire="mlie.ancientrimcore">AR_Iron</li>
				<li MayRequire="mlie.ancientrimcore">AR_Copper</li>
				<li MayRequire="mlie.ancientrimcore">AR_Bronze</li>
				<li MayRequire="tsp.isengriff.bronzecoppertin.split">ST_Copper</li>
				<li MayRequire="tsp.isengriff.bronzecoppertin.split">ST_Tin</li>
				<li MayRequire="tsp.isengriff.bronzecoppertin.split">ST_Bronze</li>
				<li MayRequire="vanillaexpanded.recycling">VRecyclingE_TrashBrick</li>
            </thingDefs>
			<disallowedThingDefs>
				<li>Asbestos_Tile</li>
			</disallowedThingDefs>
        </fixedIngredientFilter>
		<defaultIngredientFilter>
			<categories>
				<li>StoneBlocks</li>
			</categories>
			<thingDefs>
				<li>Asbestos_Fiber</li>
				<li>Steel</li>
				<li MayRequire="mlie.ancientrimcore">AR_Iron</li>
				<li MayRequire="mlie.ancientrimcore">AR_Copper</li>
				<li MayRequire="mlie.ancientrimcore">AR_Bronze</li>
				<li MayRequire="tsp.isengriff.bronzecoppertin.split">ST_Copper</li>
				<li MayRequire="tsp.isengriff.bronzecoppertin.split">ST_Tin</li>
				<li MayRequire="tsp.isengriff.bronzecoppertin.split">ST_Bronze</li>
				<li MayRequire="vanillaexpanded.recycling">VRecyclingE_TrashBrick</li>
			</thingDefs>
			<disallowedThingDefs>
				<li>Asbestos_Chunk</li>
				<li>Asbestos_Tile</li>
			</disallowedThingDefs>
		</defaultIngredientFilter>
        <products>
            <Asbestos_Tile>50</Asbestos_Tile>
        </products>
        <recipeUsers>
			<li>TableStonecutter</li>
			<li>FueledSmithy</li>
			<li>ElectricSmithy</li>
			<li>TableMachining</li>
            <li>ElectricSmelter</li>
        </recipeUsers>
    </RecipeDef>
	
