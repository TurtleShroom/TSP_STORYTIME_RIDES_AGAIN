<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!--================================ Nokia 3310 ======================================-->
  <ThingDef ParentName="RockBase">
    <defName>Mineable3310a</defName>
    <label>Compacted nokia 3310</label>
    <description>Nokia 3310 compacted in a lump.</description>
    <graphicData>
      <texPath>Things/Building/Linked/CompactedMachinery_Atlas</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <color>(110,118,120)</color>
    </graphicData>
    <statBases>
      <MaxHitPoints>4000</MaxHitPoints>
    </statBases>
    <building>
      <isResourceRock>true</isResourceRock>
      <mineableThing>nokia3310a</mineableThing>
      <mineableYield>5</mineableYield>
      <mineableScatterCommonality>0.005</mineableScatterCommonality>
      <mineableScatterLumpSizeRange>
        <min>1</min>
        <max>3</max>
      </mineableScatterLumpSizeRange>
    </building>
  </ThingDef>
