<?xml version="1.0" encoding="utf-8" ?>

<Patch>
	<Operation Class="PatchOperationConditional">
	<!-- Does this exist? -->
		<xpath>Defs/ThingDef[defName="BMT_Goeto"]</xpath>
		<!-- If it does, add this. -->
		<match Class="PatchOperationAddModExtension">
			<success>Always</success>
			<xpath>Defs/ThingDef[defName = "BMT_Goeto" or defName = "BMT_SandyToad"]</xpath>
			<value>
				<li Class="VEF.AnimalBehaviours.AnimalStatExtension">
					<statToAdd>
						<li>ST_FrogRole</li>
						<li>ST_FrogRoleSecondary</li>
						<li>ST_FrogRarity</li>
					</statToAdd>
					<statValues>
						<li>ST_FrogFoodProduction</li>
						<li>ST_FrogFriendly</li>
						<li>ST_FrogCommon</li>					
					</statValues>
					<statDescriptions>
						<li>ST_FrogFoodProductionDesc</li>
						<li>ST_FrogFriendlyDesc</li>
						<li>ST_FrogCommonDesc</li>
					</statDescriptions>
					<showImageInInfoCard>false</showImageInInfoCard>
				</li>
			</value>
		</match>
	</Operation>
</Patch>