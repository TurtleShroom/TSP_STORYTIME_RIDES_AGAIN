	<Operation Class="PatchOperationConditional">
	<!-- Does this exist? -->
		<xpath>Defs/MedievalOverhaul.SeperateHideList[defName="WhiteList"]/blackListLeathers</xpath>
		<!-- If it does, add this. -->
		<match Class="PatchOperationAdd">
			<xpath>Defs/MedievalOverhaul.SeperateHideList[defName="WhiteList"]/blackListLeathers</xpath>
			<success>Always</success>
			<value>
				<li MayRequire="TSP.Isengriff.Storytime">Leather_Cartographer</li>
				<li MayRequire="TSP.Isengriff.Storytime">VAEAP_Leather_Car</li>
				<li MayRequire="TSP.Isengriff.Storytime">Leather_Beet</li>
				<li MayRequire="TSP.Isengriff.Storytime">Leather_Bug</li>
				<li MayRequire="TSP.Isengriff.Storytime">Leather_Puffle</li>
				<li MayRequire="TSP.Isengriff.Storytime">ST_Bean</li>
				<li MayRequire="TSP.Isengriff.Storytime">ST_RouletteBean</li>
				<li MayRequire="TSP.Isengriff.Storytime">Rylex_Coconut</li> <!-- Dropped by Coconut Frogs -->
				<li MayRequire="TSP.Isengriff.Storytime">BORPA_ChunkKachorpaSlag</li>
				<li MayRequire="TSP.Isengriff.Storytime">BORPA_KachorpaSteel</li>
				<li MayRequire="TSP.Isengriff.Storytime">TSP_ST_SkeletalMarrow</li>
				<li MayRequire="TSP.Isengriff.Storytime">ST_BananaScytheCorpse</li>
			</value>
		</match>
		<!-- If the first XPATH does not exist, the original code stands. -->
	</Operation>
</Patch>