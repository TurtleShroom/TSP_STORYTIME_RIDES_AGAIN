<?xml version="1.0" encoding="utf-8" ?><Patch>	<Operation Class="PatchOperationConditional">	<!-- Does this exist? -->		<xpath>Defs/ThingDef[defName="AG_Alphapack"]</xpath>		<!-- If it does, add this. -->		<match Class="PatchOperationAdd">			<xpath>/Defs</xpath>			<success>Always</success>			<value>				<GeneDef>					<defName>TSP_AG_EggNog_Upchuck</defName>					<label>egg nog vomit</label>					<displayCategory>AG_VomitType</displayCategory>					<description>The bodies of carriers with this gene secrete proteins and hormones that permanently simulate the high of having egg nog in the bloodstream. They will constantly seek out Egg Nog. When they vomit, the bile in their stomach smells like egg nog.</description>					<iconPath>UI/Icons/Genes/TSP_AP_Upchuck_EggNog</iconPath>					<customEffectDescriptions>						<li>Permanent Egg Nog Need. The Pawn can live without it, but it won't be pleasurable for him.</li>						<li>The Pawn will vomit white egg nog instead of his usual stomach bile.</li>					</customEffectDescriptions>					<displayOrderInCategory>12</displayOrderInCategory>					<!-- <chemicalNeed>ST_EggNog_DependencyNeed</chemicalNeed> -->					<enablesNeeds>						<li>ST_EggNog_DependencyNeed</li>					</enablesNeeds>					<exclusionTags>						<li>AG_VomitType</li>						<li>VomitType</li>						<li>Vomit</li>					</exclusionTags>					<selectionWeight>0.66</selectionWeight>					<biostatCpx>1</biostatCpx>					<biostatMet>0</biostatMet>					<symbolPack>						<wholeNameSymbols>							<li>								<symbol>nogupchuck</symbol>							</li>						</wholeNameSymbols>						<prefixSymbols>							<li>								<symbol>nog</symbol>							</li>						</prefixSymbols>					</symbolPack>					<modExtensions>						<li Class="VEF.Genes.GeneExtension" MayRequire="oskarpotocki.vanillafactionsexpanded.core,sarg.alphagenes">							<backgroundPathEndogenes>UI/Icons/Genes/AG_Endogenes</backgroundPathEndogenes>							<backgroundPathXenogenes>UI/Icons/Genes/AG_Xenogenes</backgroundPathXenogenes>							<customVomitThingDef>Filth_NogBlood</customVomitThingDef>										</li>					</modExtensions>				</GeneDef>			</value>		</match>	</Operation></Patch>