		<!-- If it does, do this replacement. -->
		<match Class="PatchOperationReplace">
			<xpath>Defs/ThingDef[defName="ST_Parrot"]/comps/li/eggUnfertilizedDef</xpath>
			<success>Always</success>
			<value>
				<eggUnfertilizedDef>EggMacawUnfertilized</eggUnfertilizedDef>
			</value>
		</match>
		<!-- If the first XPATH does not exist, the original code stands. -->
    </Operation>
	
	<Operation Class="PatchOperationConditional">
	<!-- Does this exist? -->
		<xpath>Defs/ThingDef[defName="EggMacawUnfertilized"]/label</xpath>
		<!-- If it does, do this replacement. -->
		<match Class="PatchOperationReplace">
			<xpath>Defs/ThingDef[defName="EggMacawUnfertilized"]/label</xpath>
			<success>Always</success>
			<value>
				<label>parrot egg (unfert.)</label>
			</value>
		</match>
		<!-- If the first XPATH does not exist, the original code stands. -->
    </Operation>
	
	<!--
	<Operation Class="PatchOperationConditional">
	 DO THE GENTILES KNOW?
		<xpath>Defs/ThingDef[defName="Macaw"]</xpath>
		If they do, SHUT IT DOWN.
		<match Class="PatchOperationRemove">
			<xpath>Defs/ThingDef[defName="ST_Parrot"]</xpath>
			<success>Always</success>
		</match>
		If the first XPATH does not exist, the original code stands.
    </Operation>
	-->
	
	<Operation Class="PatchOperationConditional">
	<!-- DO THE GENTILES KNOW? -->
		<xpath>Defs/ThingDef[defName="Macaw"]</xpath>
		<!-- If they do, SHUT IT DOWN. -->
		<match Class="PatchOperationRemove">
			<xpath>Defs/ThingDef[defName="ST_ParrotEggUnfertilized"]</xpath>
			<success>Always</success>
		</match>
		<!-- If the first XPATH does not exist, the original code stands. -->
    </Operation>
	
	<Operation Class="PatchOperationConditional">
	<!-- Does this exist? -->
		<xpath>Defs/ThingDef[defName="Macaw"]/tradeTags</xpath>
		<!-- If it does, add this. -->
		<match Class="PatchOperationAdd">
			<xpath>Defs/ThingDef[defName="Macaw"]/tradeTags</xpath>
			<success>Always</success>
			<value>
				<li>AnimalCommon</li>
				<li>AnimalPet</li>
				<li>PiratePet</li>
				<li>StorytimeAnimal</li>
			</value>
		</match>
		<!-- If the first XPATH does not exist, the original code stands. -->
    </Operation>
	
	
