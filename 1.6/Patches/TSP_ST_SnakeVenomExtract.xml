<?xml version="1.0" encoding="utf-8" ?>

<Patch>
	<!-- Aelanna's Model No-Match Detector and Insertion Code -->
	<!-- Special thanks to Aelanna for straight-up saving me scores of headaches! -->
	<!-- Add comps field to Settlement WorldObjectDef if it doesn't exist -->
	<Operation Class="PatchOperationConditional">
		<xpath>Defs/ThingDef[defName="AA_Barbslinger"]/comps</xpath>
		<nomatch Class="PatchOperationAdd">
			<xpath>Defs/ThingDef[defName="AA_Barbslinger"]</xpath>
			<success>Always</success>
			<value>
				<comps></comps>
			</value>
		</nomatch>
	</Operation>

	<Operation Class="PatchOperationConditional">
	<!-- Does this exist? -->
	<xpath>Defs/ThingDef[defName="AEXP_Rattlesnake"]</xpath>
		<!-- If it does, add this. -->
		<match Class="PatchOperationAdd">
			<success>Always</success>
			<xpath>Defs/ThingDef[defName="AEXP_Rattlesnake"]/comps</xpath>
			<value>
				<li Class="VEF.AnimalBehaviours.CompProperties_AnimalProduct" MayRequire="oskarpotocki.vanillafactionsexpanded.core">
					<resourceDef>ST_PoisonExtract</resourceDef>
					<gatheringIntervalDays>2</gatheringIntervalDays>
					<resourceAmount>5</resourceAmount>
					<customResourceString>PoisonGather</customResourceString>
				</li>
			</value>
		</match>
		<!-- If the first XPATH does not exist, the original code stands. -->
	</Operation>
	
