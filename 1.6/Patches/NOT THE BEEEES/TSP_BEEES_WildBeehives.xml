<Patch>
	<Operation Class="PatchOperationConditional">
	<!-- Does this exist? -->
		<xpath>/Defs/ThingDef[defName="RB_WildHive_Temperate"]</xpath>
		<!-- If it does, add this. -->
		<match Class="PatchOperationAdd">
			<success>Always</success>
			<xpath>/Defs</xpath>
			<value>
				<ThingDef ParentName="RB_WildHiveBase">
					<defName>TSP_ST_RB_WildHive_Gnome</defName>
					<label>wild Gnee Hive</label>
					<description>A Gnee, short for Gnome Bee, is a special type of bee originally engineered by the Gnome peoples to produce a special delicacy called Gnome Honey. Gnee hives can be immediately identified by their bizarre green color. This particular hive occurred naturally in the wild.</description>
					<descriptionHyperlinks>
						<ThingDef>RB_Bee_TSP_ST_Gnee_Basic_Queen</ThingDef>
						<ThingDef>RB_Bee_TSP_ST_Gnee_Basic_Drone</ThingDef>
						<ThingDef>TSP_ST_RB_Gnomb_Basic</ThingDef>
						<RecipeDef>TSP_ST_RB_CentrifugeGnee1Comb</RecipeDef>
						<ThingDef>TSP_ST_GnomeHoney</ThingDef>
						<!-- <ThingDef>TSP_ST_RB_WildHive_Gnome</ThingDef> -->
						<RecipeDef>TSP_ST_RB_Gneehive_Extract</RecipeDef>
						<ThingDef MayRequire="tsp.zal.patchhoney2">TSP_LaymanHoneyMachine</ThingDef>
					</descriptionHyperlinks>
					<graphicData>
						<texPath>Things/Item/Hives/RB_WildHive_Jungle</texPath>
						<graphicClass>Graphic_Single</graphicClass>
						<color>(69,193,69)</color>
					</graphicData>
					<stackLimit>1000</stackLimit>
				</ThingDef>
				
				<RecipeDef>
					<defName>TSP_ST_RB_Gneehive_Extract</defName>
					<label>extract from wild Gnee Hive</label>
					<description>Gently extract basic Gnome Bees and their honeycombs from a wild Gnome Beehive.</description>
					<descriptionHyperlinks>
						<ThingDef>RB_Bee_TSP_ST_Gnee_Basic_Queen</ThingDef>
						<ThingDef>RB_Bee_TSP_ST_Gnee_Basic_Drone</ThingDef>
						<ThingDef>TSP_ST_RB_Gnomb_Basic</ThingDef>
						<RecipeDef>TSP_ST_RB_CentrifugeGnee1Comb</RecipeDef>
						<ThingDef>TSP_ST_GnomeHoney</ThingDef>
						<ThingDef>TSP_ST_RB_WildHive_Gnome</ThingDef>
						<!-- <RecipeDef>TSP_ST_RB_Gneehive_Extract</RecipeDef> -->
						<ThingDef MayRequire="tsp.zal.patchhoney2">TSP_LaymanHoneyMachine</ThingDef>
					</descriptionHyperlinks>
					<jobString>Extracting bees</jobString>
					<effectWorking>Cremate</effectWorking>
					<soundWorking>Recipe_Cremate</soundWorking>
					<workAmount>760</workAmount>
					<recipeUsers Inherit="False">
						<li MayRequire="sarg.rimbees">RB_Beekeeping_Workbench_Building</li>
						<li>GnomeTable</li>
						<!-- <li MayRequire="tsp.zal.patchhoney2">TSP_LaymanHoneyMachine</li> -->
					</recipeUsers>	
					<ingredients Inherit="False">
						<li>
							<filter>
								<thingDefs>
									<li>TSP_ST_RB_WildHive_Gnome</li>
								</thingDefs>
							</filter>
							<count>1</count>
						</li>
					</ingredients>
					<fixedIngredientFilter>
						<thingDefs>
							<li>TSP_ST_RB_WildHive_Gnome</li>
						</thingDefs>
					</fixedIngredientFilter>
					<products>
						<RB_Bee_TSP_ST_Gnee_Basic_Drone>2</RB_Bee_TSP_ST_Gnee_Basic_Drone>
						<RB_Bee_TSP_ST_Gnee_Basic_Queen>2</RB_Bee_TSP_ST_Gnee_Basic_Queen>
					</products>
					<skillRequirements>
						<Crafting>1</Crafting>
					</skillRequirements>
				</RecipeDef>	
			</value>
		</match>
		<!-- If the first XPATH does not exist, the original code stands. -->
	</Operation>

