<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <ThingDef ParentName="PlantFoodRawBase">
    <defName>Fruit_Coconut</defName>
    <label>coconuts</label>
    <description>The large, hard-shelled fruit of the coconut palm. The meat inside is very nutritious and safe to eat raw, but takes a long time to grow. Can be processed into coconut milk at a cooking stove.</description>
    <graphicData>
      <texPath>Things/Item/Resource/Fruit_Coconut</texPath>
    </graphicData>
    <statBases>
      <MarketValue>2</MarketValue>
      <Mass>0.5</Mass>
	  <Nutrition>0.1</Nutrition>
      <FoodPoisonChanceFixedHuman>0</FoodPoisonChanceFixedHuman>
    </statBases>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>15</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
    <ingestible>
      <preferability>RawTasty</preferability>
      <tasteThought></tasteThought>
      <foodType>VegetableOrFruit</foodType>
    </ingestible>
  </ThingDef>
</Defs>
