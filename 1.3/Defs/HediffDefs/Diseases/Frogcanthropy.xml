<Defs>
<!--Frog Parts-->

	<!--Frog Arm-->
  <HediffDef ParentName="AddedBodyPartBase">
    <defName>FrogArm</defName>
    <label>frog-like arm</label>
    <labelNoun>a froglike arm</labelNoun>
    <description>The arm has warped into one similar to a frog- very powerful.</description>
    <descriptionHyperlinks><ThingDef>FrogArm</ThingDef></descriptionHyperlinks>
    <spawnThingOnRemoved>FrogArm</spawnThingOnRemoved>
	<isBad>false</isBad>
    <tendable>false</tendable>
	<everCurableByItem>false</everCurableByItem>
	<scenarioCanAdd>false</scenarioCanAdd>
	<initialSeverity>1</initialSeverity>
	<maxSeverity>1</maxSeverity>
	<stages>
		<li>
			<becomeVisible>true</becomeVisible>
			<statOffsets>
				<SwimSpeed MayRequire="biomesteam.biomesislands">2</SwimSpeed>
			</statOffsets>
		</li>
	</stages>
    <comps>
      <li Class="HediffCompProperties_VerbGiver">
        <tools>
          <li>
            <label>frog fist</label>
            <capacities>
              <li>Blunt</li>
            </capacities>
            <power>10</power>
            <cooldownTime>2</cooldownTime>
            <soundMeleeHit>MeleeHit_BionicPunch</soundMeleeHit>
            <soundMeleeMiss>MeleeMiss_BionicPunch</soundMeleeMiss>
          </li>
        </tools>
      </li>
	  
    </comps>
    <addedPartProps>
      <solid>true</solid>
      <partEfficiency>1.2</partEfficiency>
      <betterThanNatural>true</betterThanNatural>
    </addedPartProps>
  </HediffDef>
  
  <ThingDef ParentName="BodyPartBase">
    <defName>FrogArm</defName>
    <label>frog-like arm</label>
    <description>A human arm that has been warped into one akin to a frog's forearm.</description>
    <techLevel>Medieval</techLevel>
    <thingCategories>
      <li>BodyPartsNatural</li>
    </thingCategories> 
    <graphicData>
      <texPath>Things/Item/Health/HealthItem</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>0.80</drawSize>
      <color>(140,190,110)</color>
    </graphicData>
    <tradeTags>
      <li>Bionic</li>
    </tradeTags>
    <statBases>
      <Mass>0.3</Mass>
    </statBases>
  </ThingDef>
  
  <RecipeDef ParentName="SurgeryInstallBodyPartProstheticBase">
    <defName>InstallFrogArm</defName>
    <label>install froglike arm</label>
    <description>Install a frog-like arm.</description>
    <descriptionHyperlinks>
      <ThingDef>FrogArm</ThingDef>
      <HediffDef>FrogArm</HediffDef>
    </descriptionHyperlinks>
    <jobString>Installing froglike arm.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>FrogArm</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>FrogArm</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Arm</li>
    </appliedOnFixedBodyParts>
    <addsHediff>FrogArm</addsHediff>
  </RecipeDef>


