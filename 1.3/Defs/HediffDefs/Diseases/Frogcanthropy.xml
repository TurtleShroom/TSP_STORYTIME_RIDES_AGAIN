    </addedPartProps>
	<stages>
		<li>
			<becomeVisible>true</becomeVisible>
			<mentalStateGivers>
				<li>
					<mentalState>Binging_Food</mentalState>
					<mtbDays>17</mtbDays> <!-- Slightly more than once per Quadrum, they try but fail to satisfy the insect craving -->
				</li>
			</mentalStateGivers>
		</li>
	</stages>
  </HediffDef>
  
