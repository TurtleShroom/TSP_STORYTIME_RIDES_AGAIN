    <settlementTexturePath>World/WorldObjects/TribalSettlement</settlementTexturePath>
	<leaderForceGenerateNewPawn>true</leaderForceGenerateNewPawn>
    <fixedLeaderKinds>
      <li>TSP_BeetMaster</li>
    </fixedLeaderKinds>
