<Defs>
	<RecipeDef>
		<defName>TSP_ST_DriedFrogPills</defName>
		<label>Process Dried Frog Pills (x5)</label>
		<description>This recipe heats and dries raw ingredients from butchered frogs to make Dried Frog Pills.</description>
		<jobString>Drying raw frog ingredients</jobString>
		<workAmount>300</workAmount>
		<workSpeedStat>DrugCookingSpeed</workSpeedStat>
		<workSkill>Cooking</workSkill>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Meat_ST_Bullfrog</li>
						<li>Meat_JudasFrog</li>
						<li>Leather_Frog</li>
						<li MayRequire="Oblivion.BORPA">BORPA_Milk</li>
						<li MayRequire="Oblivion.BORPA">Meat_BORPA_Borpa</li>
						<li MayRequire="Oblivion.BORPA">Meat_BORPA_AcridBorpa</li>
						<li MayRequire="Oblivion.BORPA">Meat_BORPA_Snorpa</li>
						<li MayRequire="Oblivion.BORPA">Meat_BORPA_Kachorpa</li>
						<li MayRequire="Oblivion.BORPA">Meat_BORPA_JohnBorpa</li>
						<li MayRequire="Oblivion.BORPA">BORPA_KachorpaSteel</li>
					</thingDefs>
				</filter>
				<count>8</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Meat_ST_Bullfrog</li>
				<li>Meat_JudasFrog</li>
				<li>Leather_Frog</li>
				<li MayRequire="Oblivion.BORPA">BORPA_Milk</li>
				<li MayRequire="Oblivion.BORPA">Meat_BORPA_Borpa</li>
				<li MayRequire="Oblivion.BORPA">Meat_BORPA_AcridBorpa</li>
				<li MayRequire="Oblivion.BORPA">Meat_BORPA_Snorpa</li>
				<li MayRequire="Oblivion.BORPA">Meat_BORPA_Kachorpa</li>
				<li MayRequire="Oblivion.BORPA">Meat_BORPA_JohnBorpa</li>
				<li MayRequire="Oblivion.BORPA">BORPA_KachorpaSteel</li>
			</thingDefs>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<thingDefs>
				<li>Meat_ST_Bullfrog</li>
				<li>Meat_JudasFrog</li>
				<li>Leather_Frog</li>
			</thingDefs>
		</defaultIngredientFilter>
		<recipeUsers>
			<li>ElectricStove</li>
			<li>FueledStove</li>
			<li MayRequire="morishep.tarojun.onioftherim">Oni_GroundOven</li>
			<li>GnomeTable</li>
			<li>DrugLab</li>
			<li>Campfire</li>
		</recipeUsers>
		<products>
			<DriedFrogPills>5</DriedFrogPills>
		</products>
	</RecipeDef>
	
