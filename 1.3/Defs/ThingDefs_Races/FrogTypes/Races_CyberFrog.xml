	  <wildBiomes>
			<BorealForest>0.005</BorealForest>
			<IceSheet>0.005</IceSheet>
			<Tundra>0.005</Tundra>
			<ColdBog>0.05</ColdBog>
			<SeaIce>0.005</SeaIce>
			<TemperateSwamp>0.05</TemperateSwamp>
			<TropicalSwamp>0.05</TropicalSwamp>
			<TemperateForest>0.01</TemperateForest>
			<TropicalRainforest>0.01</TropicalRainforest>
			<AridShrubland>0.0005</AridShrubland>
			<Desert>0.0005</Desert>
			<ExtremeDesert>0.0005</ExtremeDesert>
	  </wildBiomes>
	  -->
      <body Inherit="False">QuadrupedAnimalWithClawsTailAndJowl</body>
      <baseBodySize>0.35</baseBodySize>
      <baseHealthScale>4</baseHealthScale>
	  <!--
      <baseHungerRate>0.15</baseHungerRate>
      <foodType Inherit="False">OmnivoreRoughAnimal, Kibble</foodType>
	  -->
	  <useMeatFrom>ST_BullFrog</useMeatFrom>
      <leatherDef>Steel</leatherDef>
	  <!--
      <wildness>0</wildness>
      <trainability>Advanced</trainability>
      <petness>1</petness>
	  -->
      <lifeExpectancy>999</lifeExpectancy>
	  <hasGenders>false</hasGenders>
      <lifeStageAges>
        <li>
          <def>AnimalBaby</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>AnimalJuvenile</def>
          <minAge>0.05</minAge>
        </li>
        <li>
          <def>MechanoidFullyFormed</def>
          <minAge>0.1</minAge>
          <soundWounded>Pawn_Mech_Scyther_Wounded</soundWounded>
          <soundDeath>Pawn_Mech_Scyther_Death</soundDeath>
		  <soundCall>Pawn_Mech_Scyther_Call</soundCall>
          <soundAngry>Frog_Angry</soundAngry>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
    </race>
    <tradeTags>
      <!-- <li>AnimalExotic</li> -->
	  <li>AnimalInsectSpace</li>
    </tradeTags>
  </ThingDef>
  
 
