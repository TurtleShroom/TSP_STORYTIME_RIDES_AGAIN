        <defName>ProjectileHatchet</defName>
        <label>Thrown Hatchet</label>
        <graphicData>
            <texPath>Things/Items/Hatchets/BronzeHatchet</texPath>
            <graphicClass>Graphic_Single</graphicClass>
        </graphicData>
