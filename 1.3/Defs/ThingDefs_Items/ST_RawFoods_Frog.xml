<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="RoughPlantBase">
    <defName>TomatoCyst</defName>
    <label>tomato cyst</label>
    <description>Contrary to popular belief, these "tomatoes" are neither a vegetable or a fruit. These are actually cysts that grow on the back of a tomato frog. Their taste is indistinguishable from actual tomatoes.</description>
    <graphicData>
		<texPath>Things/Items/Resources/Tomato</texPath>
		<graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <statBases>
      <MarketValue>1.9</MarketValue>
      <Mass>0.045</Mass>
      <MaxHitPoints>60</MaxHitPoints>
      <Flammability>0.2</Flammability>
      <Nutrition>0.05</Nutrition>
    </statBases>
    <comps>
		<li Class="CompProperties_Rottable">
			<daysToRotStart>30</daysToRotStart>
			<rotDestroys>true</rotDestroys>
		</li>
    </comps>
    <ingestible>
		<foodType>VegetableOrFruit, AnimalProduct</foodType>
    </ingestible>
    <thingCategories>
		<li>PlantFoodRaw</li>
		<!-- <li>AnimalProductRaw</li> -->
    </thingCategories>
  </ThingDef>
</Defs>