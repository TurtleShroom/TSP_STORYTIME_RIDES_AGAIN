<?xml version="1.0" encoding="utf-8" ?><Defs>	<ProcessorFramework.ProcessDef>		<defName>ST_SpikedButterscotch_Process</defName>		<thingDef>ST_SpikedButerscotch</thingDef>		<ingredientFilter>			<thingDefs>				<li>TSP_ST_SpikedButterscotch_Wort</li>			</thingDefs>		</ingredientFilter>		<processDays>3</processDays> <!-- in days, a day is 60,000 ticks -->		<capacityFactor>1</capacityFactor>		<efficiency>0.9</efficiency>		<usesTemperature>true</usesTemperature>		<temperatureSafe>-35.0~80.0</temperatureSafe>		<temperatureIdeal>-35.0~-5.0</temperatureIdeal>		<ruinedPerDegreePerHour>0.001</ruinedPerDegreePerHour>		<speedBelowSafe>0.1</speedBelowSafe>		<speedAboveSafe>1.0</speedAboveSafe>		<sunFactor>1.0~1.0</sunFactor>		<rainFactor>1.0~1.0</rainFactor>		<snowFactor>1.0~1.0</snowFactor>		<windFactor>1.0~1.0</windFactor>		<unpoweredFactor>0.0</unpoweredFactor>		<unfueledFactor>0.0</unfueledFactor>		<powerUseFactor>1.0</powerUseFactor>		<fuelUseFactor>1.0</fuelUseFactor>		<filledGraphicSuffix></filledGraphicSuffix>		<usesQuality>false</usesQuality>		<qualityDays>(1, 2, 3, 4, 5, 6, 7)</qualityDays>		<color>(212, 136, 39)</color> <!-- 0-255 or 0.0-1.0 -->		<customLabel>spiked butterscotch</customLabel>		<destroyChance>0.0</destroyChance>		<bonusOutputs>			<Rat>(0.1, 1)</Rat>			<ST_Butterscotch>(0.1, 1)</ST_Butterscotch>		</bonusOutputs>	</ProcessorFramework.ProcessDef></Defs>