<?xml version="1.0" encoding="utf-8" ?>

<Defs>
<!-- Snorpa Body -->
  <BodyDef>
    <defName>BORPA_SnorpaBodyDef</defName>
    <label>Snorpa</label>
    <corePart>
      <def>BORPA_BorpaBody</def>
      <height>Middle</height>
      <depth>Outside</depth>
      <parts>	
		<li>
		<def>BORPA_BorpaFoot</def>
		<coverage>0.15</coverage>
		 <height>Bottom</height>
		<groups>
			<li>BORPA_FootAttackTool</li>
		</groups>
			<parts>
				<li>
				<def>Stomach</def>
				<coverage>0.05</coverage>
				<depth>Inside</depth>
				</li>
				<li>
				<def>Heart</def>
				<coverage>0.03</coverage>
				<depth>Inside</depth>
				</li>
			</parts>
		</li>
		<li>
			<def>BORPA_SnorpaShell</def>
			<coverage>0.45</coverage>
			<height>Middle</height>
			<depth>Outside</depth>
			<parts>
			<li>
				<def>Lung</def>
				<customLabel>left lung</customLabel>
				<coverage>0.03</coverage>
				<depth>Inside</depth>
				</li>
				<li>
				<def>Lung</def>
				<customLabel>right lung</customLabel>
				<coverage>0.03</coverage>
				<depth>Inside</depth>
				</li>
				<li>
				<def>Kidney</def>
				<customLabel>left kidney</customLabel>
				<coverage>0.03</coverage>
				<depth>Inside</depth>
				</li>
				<li>
				<def>Kidney</def>
				<customLabel>right kidney</customLabel>
				<coverage>0.03</coverage>
				<depth>Inside</depth>
				</li>
				<li>
				<def>Liver</def>
				<coverage>0.03</coverage>
				<depth>Inside</depth>
				</li>
			</parts>
		</li>
        
        <li>
          <def>BORPA_BorpaHead</def>
          <coverage>0.15</coverage>
          <groups>
            <li>HeadAttackTool</li>
          </groups>
          <parts>
            <li>
              <def>Skull</def>
              <coverage>0.25</coverage>
              <depth>Inside</depth>
              <parts>
                <li>
                  <def>Brain</def>
                  <coverage>0.50</coverage>
                  <depth>Inside</depth>
                </li>
              </parts>
            </li>
            <li>
              <def>Eye</def>
              <customLabel>left eye</customLabel>
              <coverage>0.10</coverage>
            </li>
            <li>
              <def>Eye</def>
              <customLabel>right eye</customLabel>
              <coverage>0.10</coverage>
            </li>
            <li>
              <def>BORPA_BorpaMouth</def>
              <coverage>0.25</coverage>
              <groups>
                <li>Mouth</li>
              </groups>
            </li>
          </parts>
        </li>
		
		
	  
	  </parts>
    </corePart>
  </BodyDef>


