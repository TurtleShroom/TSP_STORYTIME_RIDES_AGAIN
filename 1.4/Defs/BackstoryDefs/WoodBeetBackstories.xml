<?xml version="1.0" encoding="utf-8"?>

<Defs>
  <AlienRace.AlienBackstoryDef Name="BeetBackstories" Abstract="True">
	<ignoreIllegalLabelCharacterConfigError>true</ignoreIllegalLabelCharacterConfigError>
    <bodyTypeMale>Hulk</bodyTypeMale>
    <bodyTypeFemale>Female</bodyTypeFemale>
	<spawnCategories>
      <li>WoodBeetBackstory</li>
    </spawnCategories>
  </AlienRace.AlienBackstoryDef>
  
  <AlienRace.AlienBackstoryDef ParentName="BeetBackstories">
    <defName>CommunalChildWoodBeet</defName>
    <title>Child of the Wood Beet Commune</title>
	<titleShort>Communally Abused</titleShort>
    <baseDesc>[PAWN_nameDef] never knew [PAWN_possessive] parents or homeland. [PAWN_pronoun] was 'adopted' by Wood Beets and 'raised' communally. This involved a lot of drugs and other questionable parenting decisions.</baseDesc>
    <slot>Childhood</slot>
    <skillGains>
      <li>
        <key>Social</key>
        <value>3</value>
	  </li>
	  <li>
		<key>Melee</key>
        <value>3</value>
	  </li>
	  <li>
		<key>Shooting</key>
        <value>3</value>
      </li>
    </skillGains>
	<forcedTraitsChance>
		<li>
			<defName>DrugDesire</defName>
			<degree>2</degree>
		</li>
		<li>
            <defName>Psychopath</defName>
			<chance>3</chance>
        </li>
		<li>
            <defName>Cannibal</defName>
			<chance>1</chance>
        </li>
		<li>
			<defName>Bloodlust</defName>
			<chance>10</chance>
		</li>
    </forcedTraitsChance>
	<spawnCategories Inherit="False">
		<li>WoodBeetBackstory</li>
		<li>WoodBeetJoe</li>
		<li>PecanJoeBackstory</li>
		<li>Madman</li>
		<li>Sadist</li>
		<li>MedievalCriminal</li>
		<li>Raider</li>
		<li>Pirate</li>
		<li>HSAU_BS_MLN_ChildhoodGerman</li>
    </spawnCategories>
	<relationSettings>
		<relationChanceModifierLover>0</relationChanceModifierLover>
		<relationChanceModifierExLover>0</relationChanceModifierExLover>
		<relationChanceModifierFiance>0</relationChanceModifierFiance>
		<relationChanceModifierSpouse>0</relationChanceModifierSpouse>
		<relationChanceModifierExSpouse>0</relationChanceModifierExSpouse>
		<relationChanceModifierParent>0</relationChanceModifierParent>
		<relationChanceModifierChild>0</relationChanceModifierChild>
		<relationChanceModifierSibling>0</relationChanceModifierSibling>
	</relationSettings>
  </AlienRace.AlienBackstoryDef>
  
  <AlienRace.AlienBackstoryDef ParentName="BeetBackstories">
    <defName>LonelyChildWoodBeet</defName>
    <title>Lone Beet</title>
    <baseDesc>[PAWN_nameDef] was abandoned by [PAWN_possessive] parents in the middle of nowhere. Contrary to what one might expect, this actually meant [PAWN_pronoun] had a better childhood than most Wood Beets. </baseDesc>
    <slot>Childhood</slot>
    <skillGains>
      <li>
        <key>Social</key>
        <value>-3</value>
	  </li>
	  <li>
		<key>Intellectual</key>
        <value>3</value>
	  </li>
	  <li>
		<key>Animals</key>
        <value>5</value>
      </li>
	  <li>
		<key>Crafting</key>
        <value>3</value>
      </li>
	  <li>
		<key>Cooking</key>
        <value>3</value>
      </li>
	  <li>
		<key>Plants</key>
        <value>3</value>
      </li>
    </skillGains>
	 <disallowedTraitsChance>
		<li>
            <defName>Psychopath</defName>
        </li>
		<li>
            <defName>Cannibal</defName>
        </li>
		<li>
            <defName>Bloodlust</defName>
        </li>
    </disallowedTraitsChance>
	<spawnCategories>
		<li>Uncommon</li>
	</spawnCategories>
	<relationSettings>
		<relationChanceModifierLover>0</relationChanceModifierLover>
		<relationChanceModifierFiance>0</relationChanceModifierFiance>
		<relationChanceModifierSpouse>0</relationChanceModifierSpouse>
		<relationChanceModifierParent>0</relationChanceModifierParent>
		<relationChanceModifierChild>0</relationChanceModifierChild>
		<relationChanceModifierSibling>0</relationChanceModifierSibling>
	</relationSettings>
  </AlienRace.AlienBackstoryDef>
  
  <AlienRace.AlienBackstoryDef ParentName="BeetBackstories">
    <defName>ParentedChildWoodBeet</defName>
    <title>Loved Beet</title>
	<titleShort>good kid</titleShort>
    <baseDesc>[PAWN_nameDef] was one of the rare Wood Beets whose parents actually cared enough to raise their children. Because of this, [PAWN_pronoun] had a stable, functional family life.</baseDesc>
    <slot>Childhood</slot>
    <skillGains>
      <li>
		<key>Cooking</key>
        <value>4</value>
	  </li>
	  <li>
		<key>Social</key>
        <value>6</value>
      </li>
	  <li>
		<key>Crafting</key>
        <value>4</value>
      </li>
    </skillGains>
	 <disallowedTraitsChance>
        <li>
            <defName>Bloodlust</defName>
        </li>
		<li>
            <defName>Psychopath</defName>
        </li>
		<li>
            <defName>Cannibal</defName>
        </li>
    </disallowedTraitsChance>
  </AlienRace.AlienBackstoryDef>
  
  <AlienRace.AlienBackstoryDef ParentName="BeetBackstories">
    <defName>DrugAddledWoodBeet</defName>
    <title>socially competent druggie</title>
	<titleShort>Drug Addled</titleShort>
    <baseDesc>[PAWN_nameDef] has spent most of [PAWN_possessive] adult life doing various drugs. [PAWN_pronoun] is not very smart, but [PAWN_pronoun] had a certain charm.</baseDesc>
    <slot>Adulthood</slot>
    <skillGains>
      <li>
        <key>Social</key>
        <value>5</value>
	  </li>
    </skillGains>
	<forcedTraitsChance>
      <li>
        <defName>DrugDesire</defName>
        <degree>2</degree>
      </li>
    </forcedTraitsChance>
	<workDisables>
      <li>Intellectual</li>
	  <li>Caring</li>
    </workDisables>
	<spawnCategories>
      <li>WoodBeetBackstory</li>
	  <li>Outsider</li>
	  <li>Madman</li>
	  <li>Pirate</li>
	  <li>Offworld</li>
	  <li>Outlander</li>
	  <li>JeubBackstory</li>
	  <li>ESCP_DunmerTelvanniBackstory</li>
	  <li>ESCP_DunmerBackstory</li>
	  <li>ESCP_DunmerSlaveBackstory</li>
    </spawnCategories>
  </AlienRace.AlienBackstoryDef>
  
  <AlienRace.AlienBackstoryDef ParentName="BeetBackstories">
    <defName>CannibalBeet</defName>
    <title>Sapient Beet Eater</title>
	<titleShort>devourer</titleShort>
    <baseDesc>[PAWN_nameDef] loves the taste of beets, and not just the vegetable  kind! [PAWN_pronoun] has eaten a large number of sapient beets over the years, and is adept at catching and cooking them. This "pallate" has extended to any other sapient life.</baseDesc>
    <slot>Adulthood</slot>
    <skillGains>
      <li>
        <key>Melee</key>
        <value>8</value>
	  </li>
	  <li>
		<key>Social</key>
        <value>-3</value>
	  </li>
	  <li>
		<key>Cooking</key>
        <value>7</value>
      </li>
    </skillGains>
	<forcedTraitsChance>
      <li>
        <defName>Cannibal</defName>
      </li>
	  <li>
		<defName>Psychopath</defName>
	  </li>
    </forcedTraitsChance>
	<spawnCategories Inherit="False">
		<li>WoodBeetBackstory</li>
		<li>Raider</li>
		<li>TribalHunter</li>
		<li>Sadist</li>
		<li>Madman</li>
		<li>Mate</li>
		<li>HSAU_BS_MLN_AdultArtVampSoldier</li>
	</spawnCategories>
  </AlienRace.AlienBackstoryDef>
  
  <AlienRace.AlienBackstoryDef ParentName="BeetBackstories">
    <defName>ShamanWoodBeet</defName>
    <title>Pecan Shaman</title>
    <baseDesc>[PAWN_nameDef] is a Pecan Shaman, one of the religious leaders whose job it is to determine the identity of the next Pecan Joe.</baseDesc>
    <slot>Adulthood</slot>
    <skillGains>
      <li>
		<key>Social</key>
        <value>6</value>
	  </li>
	  <li>
		<key>Intellectual</key>
        <value>6</value>
	  </li>
    </skillGains>
  </AlienRace.AlienBackstoryDef>
  
  <AlienRace.AlienBackstoryDef ParentName="BeetBackstories">
    <defName>RaiderWoodBeet</defName>
    <title>Valenwood Raider</title>
	<titleShort>Raider</titleShort>
    <baseDesc>[PAWN_nameDef] has participated in numerous raids on the evil Valenroff holdings. Dozens have died by [PAWN_possessive] hand.</baseDesc>
    <slot>Adulthood</slot>
    <skillGains>
      <li>
		<key>Melee</key>
        <value>6</value>
	  </li>
	  <li>
		<key>Shooting</key>
        <value>6</value>
      </li>
    </skillGains>
	<forcedTraitsChance>
      <li>
        <defName>Bloodlust</defName>
      </li>
    </forcedTraitsChance>
	<spawnCategories Inherit="False">
		<li>BeetnikhBackstory</li>
		<li>WoodBeetBackstory</li>
		<li>JeubBackstory</li>
		<li>Raider</li>
		<li>Pirate</li>
		<li>Tribal</li>
		<li>TribalHunter</li>
