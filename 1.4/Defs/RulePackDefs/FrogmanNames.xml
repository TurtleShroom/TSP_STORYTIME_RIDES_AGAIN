<?xml version="1.0" encoding="utf-8" ?>

<Defs>
	<RulePackDef>
		<defName>FrogPersonMaleName</defName>
		<rulePack>
			<rulesStrings>
				<li>name->[Frog_MaleList] [FrogMale_FamilyList]</li>
			</rulesStrings>
			<rulesRaw>
				<li Class="Rule_File">
					<keyword>Frog_MaleList</keyword>
					<path>FrogPersonNames/FrogPersonMale</path>
				</li>
				<li Class="Rule_File">
					<keyword>FrogMale_FamilyList</keyword>
					<path>FrogPersonNames/FrogPersonLastNames</path>
				</li>
			</rulesRaw>
		</rulePack>
	</RulePackDef>

	<RulePackDef>
		<defName>FrogPersonFemaleName</defName>
		<rulePack>
			<rulesStrings>
				<li>name->[Frog_FemaleList] [FrogFemale_FamilyList]</li>
			</rulesStrings>
			<rulesRaw>
				<li Class="Rule_File">
					<keyword>Frog_FemaleList</keyword>
					<path>FrogPersonNames/FrogPersonFemale</path>
				</li>
				<li Class="Rule_File">
					<keyword>FrogFemale_FamilyList</keyword>
					<path>FrogPersonNames/FrogPersonLastNames</path>
				</li>
			</rulesRaw>
		</rulePack>
	</RulePackDef>
	
	<RulePackDef>
    <defName>FrogPersonSettlementName</defName>
    <rulePack>
      <rulesStrings>
        <li>name-&gt;[FrogTownPack]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>FrogTownPack</keyword>
          <path>FrogPersonNames/FrogPersonSettlement</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
	
	
</Defs>