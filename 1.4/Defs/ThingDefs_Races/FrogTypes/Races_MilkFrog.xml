      <MoveSpeed>3.0</MoveSpeed>
      <MarketValue>300</MarketValue>
      <ComfyTemperatureMin>-20</ComfyTemperatureMin>
      <FilthRate>10</FilthRate>
    </statBases>
    <comps>
		<li Class="CompProperties_Milkable">
			<milkDef>Milk</milkDef>
			<milkIntervalDays>1</milkIntervalDays>
			<milkAmount>10</milkAmount>
		</li>
		<li Class="AnimalBehaviours.CompProperties_FixedGender">
			<gender>Female</gender>
		</li>
		<li Class="AnimalBehaviours.CompProperties_AsexualReproduction">
			<reproductionIntervalDays>9</reproductionIntervalDays>
			<customString>Budding in: </customString>
		</li>
    </comps>
    <tools>
      <li>
        <label>left claw</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
