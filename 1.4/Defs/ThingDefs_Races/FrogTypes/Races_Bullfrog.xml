      <MoveSpeed>4.0</MoveSpeed>
      <MarketValue>250</MarketValue>
      <ComfyTemperatureMin>-9</ComfyTemperatureMin> <!-- Realistic frog temperatures -->
      <ComfyTemperatureMax>41</ComfyTemperatureMax>
	  <MeatAmount>75</MeatAmount>
    </statBases>
