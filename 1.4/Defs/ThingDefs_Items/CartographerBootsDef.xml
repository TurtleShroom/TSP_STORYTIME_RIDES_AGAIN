		<generateCommonality>0.03</generateCommonality>
		<recipeMaker>
		  <researchPrerequisite>ComplexClothing</researchPrerequisite>
		</recipeMaker>
		<graphicData>
		  <texPath>Things/Items/Cartographer_Shoes</texPath>
		  <graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<costList>
		  <Leather_Cartographer>40</Leather_Cartographer>
		</costList>
		<apparel>
			<developmentalStageFilter>Baby, Child, Adult</developmentalStageFilter>
		  <careIfWornByCorpse>false</careIfWornByCorpse>
		  <countsAsClothingForNudity>false</countsAsClothingForNudity>
		  <bodyPartGroups>
			<li>Feet</li>
		  </bodyPartGroups>
		  <layers>
			<li>Belt</li>
		  </layers>
		  <defaultOutfitTags>
			<li>Worker</li>
			<li>Soldier</li>
		  </defaultOutfitTags>
		  <tags>
			<li>ExoticMisc</li>
			<li>Spacer</li>
			<li>SpacerMilitary</li>
			<!-- <li>IndustrialAdvanced</li> -->
			<li>IndustrialMilitaryAdvanced</li>
			<li>zori</li>
			<li>WeebElite</li>
			<li>AnimeProtagonist</li>
			<li>Pirate</li>
			<li>SixthHouseSwag</li>
			<li>EVA</li>
		  </tags>
		</apparel>
		<statBases>
		  <MaxHitPoints>2000</MaxHitPoints>
		  <Mass>0.25</Mass>
		  <WorkToMake>12000</WorkToMake>
		  <EquipDelay>0.8</EquipDelay>
		  <StuffEffectMultiplierInsulation_Cold>0.5</StuffEffectMultiplierInsulation_Cold>
		  <StuffEffectMultiplierInsulation_Heat>0.5</StuffEffectMultiplierInsulation_Heat>
		</statBases>
		<equippedStatOffsets>
		  <MoveSpeed>3</MoveSpeed>
		</equippedStatOffsets>
		<thingCategories Inherit="False">
		  <li>ApparelUtility</li>
		  <li>Artifacts</li>
		  <li MayRequire="vanillaexpanded.vappe">VAE_BootsAndGloves</li>
		</thingCategories>
		<comps>
			<li Class="CompProperties_Art">
				<nameMaker>NamerArtWeaponMelee</nameMaker>
				<descriptionMaker>ArtDescription_WeaponMelee</descriptionMaker>
				<minQualityForArtistic>Awful</minQualityForArtistic>
			</li>
		</comps>
		<inspectorTabs>
			<li>ITab_Art</li>
		</inspectorTabs>
		<colorGenerator Class="ColorGenerator_StandardApparel"></colorGenerator>
