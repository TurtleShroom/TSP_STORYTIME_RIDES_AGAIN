<?xml version="1.0" encoding="utf-8" ?>

<Defs> 
 <ThingDef ParentName="RoughPlantBase">
    <defName>HarralanderLeaves</defName>
    <label>harra leaves</label>
    <description>Raw, cut leaves of a harra plant.</description>
    <graphicData>
      <texPath>Things/Items/Resources/Herbs/HarralanderLeaves</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <Nutrition>0.02</Nutrition>
      <MarketValue>1.4</MarketValue>
      <Flammability>1.3</Flammability>
    </statBases>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>30</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
    <thingCategories Inherit="False">
      <li>PlantFoodRaw</li>
	  <li MayRequire="mlie.takisrandomstuff">MedicalHerbs</li>
    </thingCategories>
    <ingestible>
      <preferability>DesperateOnly</preferability>
      <canAutoSelectAsFoodForCaravan>false</canAutoSelectAsFoodForCaravan>
	  <specialThoughtAsIngredient>TSP_ST_HarraSpice</specialThoughtAsIngredient>
    </ingestible>
  </ThingDef>  
  
