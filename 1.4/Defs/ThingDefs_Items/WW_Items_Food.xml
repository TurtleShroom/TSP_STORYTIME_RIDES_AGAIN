<?xml version="1.0" encoding="utf-8" ?>

<Defs>
  <ThingDef ParentName="OrganicProductBase">
    <defName>Sloodge</defName>
    <label>sloodge</label>
    <description>A preserved mashed mixture of fat and plant food. Tastes bland, but not offensive. It lasts a very long time without refrigeration, and has just enough nutrients to be worth eating and, unlike Kibble, is still salvagable as an ingredient.</description>
    <thingClass>ThingWithComps</thingClass>
    <thingCategories>
      <li>Foods</li>
	  <li>AnimalProductRaw</li>
    </thingCategories>
    <graphicData>
      <texPath>Items/Sloodge</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <socialPropernessMatters>true</socialPropernessMatters>
    <statBases>
      <MarketValue>4</MarketValue>
      <Mass>0.018</Mass>
      <Flammability>0</Flammability>
      <WorkToMake>70</WorkToMake>
      <DeteriorationRate>3</DeteriorationRate>
      <Nutrition>0.39</Nutrition>
    </statBases>
    <ingestible>
      <foodType>Meal, Processed, CarnivoreAnimalStrict, VegetarianAnimalStrict, Fluid, Kibble</foodType>
      <preferability>MealSimple</preferability>
      <ingestEffect>EatMeat</ingestEffect>
      <ingestSound>Meal_Eat</ingestSound>
      <!-- <optimalityOffsetHumanlikes>0</optimalityOffsetHumanlikes> -->
    </ingestible>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>180</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
      <li Class="CompProperties_Ingredients">
        <noIngredientsFoodKind>Meat</noIngredientsFoodKind>
      </li>
      <!-- <li Class="CompProperties_FoodPoisonable" /> -->
    </comps>
    <allowedArchonexusCount>20000</allowedArchonexusCount>
  </ThingDef>

</Defs>
