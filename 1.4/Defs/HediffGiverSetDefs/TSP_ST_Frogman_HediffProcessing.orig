<Defs>
  <HediffGiverSetDef>
	<defName>TSP_ST_Frogman_Flavor_Processing</defName>
	
	<!-- Code derived entirely, and courtesy of, the Oni Mod by Lucky, Mori Ginto, and Tarojun -->
	<hediffGivers>
		<!-- Unlike Werefrogs, Frogmen have the Hediffs built into them, so only a few Hediffs that give special abilities need to be added. -->
		<li Class="HediffGiver_Birthday">
			<hediff>FrogTongue</hediff>
			<ageFractionChanceCurve>
				<points>
					<li>(0, 1)</li> <!-- Always -->
					<li>(1, 1)</li>
				</points>
			</ageFractionChanceCurve>
		</li>
	</hediffGivers>
  </HediffGiverSetDef>
</Defs>