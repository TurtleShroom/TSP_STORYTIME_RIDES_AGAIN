<Defs>
  <RecipeDef>
    <defName>Make_WormCrunchies</defName>
    <label>make worm crunchies</label>
    <description>Bake a mix of worms and baked goods with the intent of grinding them into a tasty snack.</description>
	<descriptionHyperlinks>
		<RecipeDef>TSP_ST_ProcessWormCrunchies_One</RecipeDef>
		<RecipeDef>TSP_ST_ProcessWormCrunchies_Five</RecipeDef>
		<RecipeDef>TSP_ST_ProcessWormCrunchies_Fifteen</RecipeDef>
		<RecipeDef>TSP_ST_ProcessWormCrunchies_Twenty</RecipeDef>
		<RecipeDef>TSP_ST_ProcessWormCrunchies_Thirty</RecipeDef>
		<ThingDef>WormCrunchies</ThingDef>
	</descriptionHyperlinks>
    <jobString>Baking a sheet of Worm Crunchies</jobString>
    <workAmount>600</workAmount>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <ingredients>
      <li>
        <filter>
					<thingDefs>
						<li>ST_Worm</li>
					</thingDefs>
        </filter>
        <count>35</count>
      </li>
	  <li>
        <filter>
					<customSummary>cereals, Gnome Sugar, sweeteners</customSummary>
					<categories>
						<li MayRequire="Mlie.TakisRandomStuff">Cereal</li>
					</categories>
					<thingDefs>
						<li>ST_GnomeSugar</li>
						<li MayRequire="vanillaexpanded.vcooke">VCE_RawSugar</li>
						<li MayRequire="sarg.alphabiomes">AB_RawSugarFamewort</li>
						<li MayRequire="Mlie.TakisRandomStuff">RawBarley</li>
						<li MayRequire="Mlie.TakisRandomStuff">RawOats</li>
						<li MayRequire="aratocat.wonderland">Chocofuel</li>
					</thingDefs>
					<disallowedThingDefs>
						<li>GnomeFlour</li>
						<li>GnomeFlourMeal</li>
					</disallowedThingDefs>
        </filter>
        <count>15</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
					<categories>
						<li MayRequire="Mlie.TakisRandomStuff">Cereal</li>
					</categories>
					<thingDefs>
						<li>ST_Worm</li>
						<li>ST_GnomeSugar</li>
						<li MayRequire="vanillaexpanded.vcooke">VCE_RawSugar</li>
						<li MayRequire="sarg.alphabiomes">AB_RawSugarFamewort</li>
						<li MayRequire="Mlie.TakisRandomStuff">RawBarley</li>
						<li MayRequire="Mlie.TakisRandomStuff">RawOats</li>
						<li MayRequire="aratocat.wonderland">Chocofuel</li>
					</thingDefs>
					<disallowedThingDefs>
						<li>GnomeFlour</li>
						<li>GnomeFlourMeal</li>
					</disallowedThingDefs>
    </fixedIngredientFilter>
    <defaultIngredientFilter>
					<thingDefs>
						<li>ST_Worm</li>
						<li>ST_GnomeSugar</li>
					</thingDefs>
    </defaultIngredientFilter>
	<recipeUsers>
    		<li>GnomeTable</li>
	</recipeUsers>
	<researchPrerequisite>gnomeCooking</researchPrerequisite>
	<products>
		<TSP_ST_WormCrunchiesRaw>5</TSP_ST_WormCrunchiesRaw>
	</products>
