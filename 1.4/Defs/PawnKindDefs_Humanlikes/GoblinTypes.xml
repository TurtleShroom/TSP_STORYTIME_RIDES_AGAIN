	<PawnKindDef ParentName="BaseGoblinKind">
		<defName>PPHHYY_Doomsday_Goblin</defName>
		<label>goblin mad scientist</label>
		<combatPower>350</combatPower>
		<factionLeader>false</factionLeader>
		<chemicalAddictionChance>0.33</chemicalAddictionChance>
		<canBeSapper>true</canBeSapper>
		<forceNormalGearQuality>true</forceNormalGearQuality>
		<apparelMoney>
			<min>1350</min>
			<max>4200</max>
		</apparelMoney>
		<invFoodDef MayRequire="robtrg.tf2.sandvich">robtrg.tf2.sandvich</invFoodDef>
		<invNutrition MayRequire="robtrg.tf2.sandvich">12</invNutrition>
		<apparelTags Inherit="False">
			<li>Military</li>
			<li>IndustrialMilitary</li>
			<li>IndustrialMilitaryAdvanced</li>
			<li>MedievalArmorElite</li>
			<li>GoblinWear</li>
			<li>Cape</li>
		</apparelTags>
		<weaponMoney>
			<min>1500</min>
			<max>4000</max>
		</weaponMoney>
		<backstoryCryptosleepCommonality>0.061</backstoryCryptosleepCommonality>
		<techHediffsMoney>
			<min>1150</min>
			<max>3550</max>
		</techHediffsMoney>
		<techHediffsTags>
			<li>Poor</li>
			<li>Common</li>
			<li>ImplantEmpireCommon</li>
		</techHediffsTags>
		<techHediffsRequired>
			<li>DeathAcidifier</li>
			<li>VenomFangs</li>
			<li>VenomTalon</li>
			<li>VenomTalon</li>
		</techHediffsRequired>
		<apparelRequired Inherit="False">
			<li MayRequire="vanillaexpanded.vappe">VAE_Apparel_LabCoat</li>
			<li MayRequire="ludeon.rimworld.biotech">Apparel_GasMask</li>
		</apparelRequired>
		<biocodeWeaponChance>0.9</biocodeWeaponChance>
		<techHediffsChance>0.66</techHediffsChance>
		<weaponTags Inherit="False">
			<li>GoblinDoomsday</li>
			<li>GoblinTech</li>
			<li>Gun_ToxicNeedle</li>
			<li>GunHeavy</li>
			<li>SpacerGun</li>
			<li>MiniToxSpray</li>
		</weaponTags>
		<initialWillRange>5~9</initialWillRange>
		<initialResistanceRange>18~40</initialResistanceRange>
		<skills>
			<li>
				<skill>Intellectual</skill>
				<range>12~15</range>
			</li>
			<li>
				<skill>Shooting</skill>
				<range>4~5</range> <!-- Backstory adds a LOT more -->
			</li>
			<li>
				<skill>Melee</skill>
				<range>10</range>
			</li>
		</skills>
		<disallowedTraits>
			<li>Wimp</li>
			<li>Nudist</li>
			<li>BodyPurist</li>
			<li>ST_Stupid</li>
		</disallowedTraits>
		<!--
		<backstoryCategories Inherit="False">
			<li>GoblinDoomsday</li>
			<li>GoblinDoomsday_Child</li>
			<li>GoblinDoomsday_Adult</li>
		</backstoryCategories>
		-->
