	<!-- ___________________________________________________________________________________________ -->
	
	<!-- Adds Leviathan Frogs as acceptable pack animals -->
	<Operation Class="PatchOperationConditional">
	<!-- Does this exist? -->
	<xpath>/Defs/BiomeDef[defName="COMIGO_GreaterSwamp_Temperate"]</xpath>
		<!-- If it does, add this. -->
		<match Class="PatchOperationAdd">
			<success>Always</success>
			<xpath>/Defs/BiomeDef[defName="COMIGO_GreaterSwamp_Temperate"]/allowedPackAnimals</xpath>
			<value>
				<li>LeviathanFrog</li>
			</value>
		</match>
		<!-- If the first XPATH does not exist, the original code stands. -->
	</Operation>
	
