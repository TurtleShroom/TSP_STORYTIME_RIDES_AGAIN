<?xml version="1.0" encoding="utf-8" ?><Patch>	<Operation Class="PatchOperationConditional">	<!-- Does this exist? -->		<xpath>/Defs/PreceptDef[defName="VME_Alcohol_Demanded"]</xpath>		<!-- If it does, add this. -->		<match Class="PatchOperationAdd">			<success>Always</success>			<xpath>/Defs/VanillaMemesExpanded.SupportedDrinksForPreceptDefs[defName="VME_SupportedDrinksForPreceptDef"]/supportedDrinksForPrecept</xpath>			<value>				<li>ST_KrakenRum</li>				<li>ST_EggNog</li>				<li>ST_FogNog</li>				<li>ST_FieryNog</li>				<li>BlurberrySpecial</li>				<li>ShortGreenGuy</li>				<li>ChocSaturday</li>				<li>GnomeBeer</li>			</value>		</match>		<!-- If the first XPATH does not exist, the original code stands. -->	</Operation></Patch>