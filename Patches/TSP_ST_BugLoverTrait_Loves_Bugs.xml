		<match Class="PatchOperationAdd">
			<success>Always</success>
			<xpath>/Defs/ThoughtDef[defName="AteInsectMeatDirect"]/nullifyingTraits</xpath>
			<value>
				<li>Buglover</li>
			</value>
		</match>
