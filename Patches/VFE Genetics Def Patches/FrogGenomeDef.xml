<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<Operation Class="PatchOperationFindMod">
		<!-- Is this Mod installed? -->
		<mods>
			<li>Vanilla Genetics Expanded</li>
		</mods>
		<!-- If it is, add this. -->
		<match Class="PatchOperationAdd">
			<success>Always</success>
			<xpath>/Defs</xpath>
			<value>
				<GeneticRim.ExtractableAnimalsList>
					<defName>ST_TierOneFrogList</defName>
					<tier>1</tier>
					<itemProduced>ST_FrogGenetic</itemProduced>
					<extractableAnimals>
						<li>ST_Toad</li>
						<li>ST_TreeFrog</li>			
						<li>PepsiFrog</li>
						<li>JudasFrog</li>	
						<li>BloodFrog</li>
						<li>FireBelliedToad</li>	
						<li>ST_BullFrog</li>
						<li>LeviathanFrog</li>	
						<li>CyberFrogHostile</li>
						<li>CyberFrogDocile</li>
						<li>WoodFrog</li>	
						<li>PoisonDartFrog</li>
						<li>TomatoFrog</li>
						<li>SpringPeeper</li>
						<li>MilkFrog</li>
						<li>BudgettFrog</li>
						<li>Werefrog</li>
						<li>Corpse_Werefrog</li>
						<li MayRequire="hc.animal.2">HC_Bullfrog</li>
						<li MayRequire="crazedmonkey231.frug">Frug</li>
					</extractableAnimals>
				</GeneticRim.ExtractableAnimalsList>
				
				<ThingDef ParentName="GR_GeneticsBase">
					<defName>ST_FrogGenetic</defName>
					<graphicData>
						<texPath>Things/Items/Resources/Genomes/GenomeFrog</texPath>
					</graphicData>
					<label>frog genome</label>
					<description>A sample of DNA derived from a living frog. It can be used at the geno-morpher to create a growth cell of a hybrid animal. This Tier I genome can be used as either a primary or secondary genome.</description>
				</ThingDef>

				<RecipeDef>
					<defName>ST_ExtractGenesFrog</defName>
					<label>extract genes from a frog</label>
					<description>Extract genetic material from a frog.</description>
					<jobString>Extracting Genetic Material</jobString>
					<effectWorking>Cremate</effectWorking>
					<soundWorking>Recipe_Cremate</soundWorking>
					<workAmount>2500</workAmount>
					<workSkill>Intellectual</workSkill>
					<workSpeedStat>ResearchSpeed</workSpeedStat>
					<recipeUsers>
						<li>GR_GeneticExtractionTable</li>	  
					</recipeUsers>	
					<ingredients>
						<li>
							<filter>
								<thingDefs>
									<li>ST_Toad</li>
									<li>ST_TreeFrog</li>			
									<li>PepsiFrog</li>
									<li>JudasFrog</li>	
									<li>BloodFrog</li>
									<li>FireBelliedToad</li>	
									<li>ST_BullFrog</li>
									<li>LeviathanFrog</li>	
									<li>CyberFrogHostile</li>
									<li>CyberFrogDocile</li>
									<li>WoodFrog</li>	
									<li>PoisonDartFrog</li>
									<li>TomatoFrog</li>
									<li>SpringPeeper</li>
									<li>MilkFrog</li>
									<li>BudgettFrog</li>
									<li>Werefrog</li>
									<li>Corpse_Werefrog</li>
									<li MayRequire="hc.animal.2">HC_Bullfrog</li>
									<li MayRequire="crazedmonkey231.frug">Frug</li>
								</thingDefs>
							</filter>
							<count>1</count>
						</li>
					</ingredients>
					<fixedIngredientFilter>
						<thingDefs>
							<li>ST_Toad</li>
							<li>ST_TreeFrog</li>			
							<li>PepsiFrog</li>
							<li>JudasFrog</li>	
							<li>BloodFrog</li>
							<li>FireBelliedToad</li>	
							<li>ST_BullFrog</li>
							<li>LeviathanFrog</li>	
							<li>CyberFrogHostile</li>
							<li>CyberFrogDocile</li>
							<li>WoodFrog</li>	
							<li>PoisonDartFrog</li>
							<li>TomatoFrog</li>
							<li>SpringPeeper</li>
							<li>MilkFrog</li>
							<li>BudgettFrog</li>
							<li>Werefrog</li>
							<li>Corpse_Werefrog</li>
							<li MayRequire="hc.animal.2">HC_Bullfrog</li>
							<li MayRequire="crazedmonkey231.frug">Frug</li>
						</thingDefs>
					</fixedIngredientFilter>
					<products>
						<ST_FrogGenetic>1</ST_FrogGenetic>
					</products>
					<skillRequirements>
						<Intellectual>6</Intellectual>
					</skillRequirements>
				</RecipeDef>
			</value>
		</match>
	</Operation>
</Defs>