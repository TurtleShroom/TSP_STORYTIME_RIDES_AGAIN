<?xml version="1.0" encoding="utf-8" ?><Patch>	<Operation Class="PatchOperationConditional">	<!-- Does this exist? -->	<xpath>/Defs/ThingDef[defName="MAG_ArchotechCheese"]</xpath>		<!-- If it does, add this. -->		<match Class="PatchOperationAdd">			<success>Always</success>			<xpath>/Defs/ThingDef[defName="MAG_ArchotechCheese"]</xpath>			<value>				<thingCategories>					<li>VCE_Cheese</li>				</thingCategories>			</value>		</match>		<!-- If the first XPATH does not exist, the original code stands. -->	</Operation>		<Operation Class="PatchOperationConditional">	<!-- Does this exist? -->	<xpath>/Defs/ThingDef[defName="VCE_Cheese"]</xpath>		<!-- If it does, do this replacement. -->		<match Class="PatchOperationReplace">			<success>Always</success>			<xpath>Defs/RecipeDef[defName="ST_Make_Cheese"]/researchPrerequisites</xpath>			<value>				<researchPrerequisites>					<li>VCE_CheeseMaking</li>				</researchPrerequisites>			</value>		</match>		<!-- If the first XPATH does not exist, the original code stands. -->	</Operation></Patch>