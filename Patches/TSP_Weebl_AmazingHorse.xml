<?xml version="1.0" encoding="utf-8" ?><Patch>	<!-- LOOK AT MY HORSE, MY HORSE IS AMAZING -->			<Operation Class="PatchOperationConditional">	<!-- Does this exist? -->		<xpath>/Defs/ThingDef[defName="Horse"]</xpath>		<!-- If it does, do this replacement. -->		<match Class="PatchOperationAdd">			<success>Always</success>			<xpath>/Defs/ThingDef[defName="Horse"]</xpath>			<value>				<comps>					<li Class="AnimalBehaviours.CompProperties_AnimalProduct">						<resourceDef>ST_MikesLemonade</resourceDef>						<gatheringIntervalDays>1.2</gatheringIntervalDays>						<resourceAmount>1</resourceAmount>						<customResourceString>TSP_WeeblAmazingHorse_Lemonade_Deposit</customResourceString> <!-- I GUESS I'D BETTER NOT SHOW YOU WHERE THE LEMONADE IS MADE -->					</li>				</comps>			</value>		</match>		<!-- If the first XPATH does not exist, the original code stands. -->	</Operation>				<!-- GET ON MY HORSE, I'LL TAKE YOU THROUGH THE UNIVERSE -->	<!-- AND ALL OF THE OTHER PLACES, TOO -->				<Operation Class="PatchOperationConditional">	<!-- Does this exist? -->		<xpath>/Defs/ThingDef[defName="MG_Horse"]</xpath>		<!-- If it does, do this replacement. -->		<match Class="PatchOperationAdd">			<success>Always</success>			<xpath>/Defs/ThingDef[defName="MG_Horse"]</xpath>			<value>				<comps>										<li Class="AnimalBehaviours.CompProperties_AnimalProduct">						<resourceDef>ST_MikesLemonade</resourceDef>						<gatheringIntervalDays>1.2</gatheringIntervalDays>						<resourceAmount>1</resourceAmount>						<customResourceString>TSP_WeeblAmazingHorse_Lemonade_Deposit</customResourceString> <!-- I GUESS I'D BETTER NOT SHOW YOU WHERE THE LEMONADE IS MADE -->					</li>				</comps>			</value>		</match>		<!-- If the first XPATH does not exist, the original code stands. -->	</Operation>		<Operation Class="PatchOperationConditional">	<!-- Does this exist? -->		<xpath>/Defs/ThingDef[defName="MG_Horse"]</xpath>		<!-- If it does, do this replacement. -->		<match Class="PatchOperationAdd">			<success>Always</success>			<xpath>/Defs/ThingDef[defName="MG_Horse"]/race</xpath>			<value>				<useMeatFrom>Horse</useMeatFrom>			</value>		</match>		<!-- If the first XPATH does not exist, the original code stands. -->	</Operation>		<Operation Class="PatchOperationConditional">	<!-- Does this exist? -->		<xpath>/Defs/ThingDef[defName="Gulden_Animal_Capricorn"]</xpath>		<!-- If it does, do this replacement. -->		<match Class="PatchOperationAdd">			<success>Always</success>			<xpath>/Defs/ThingDef[defName="Gulden_Animal_Capricorn"]</xpath>			<value>				<comps>										<li Class="AnimalBehaviours.CompProperties_AnimalProduct">						<resourceDef>ST_MikesLemonade</resourceDef>						<gatheringIntervalDays>1.2</gatheringIntervalDays>						<resourceAmount>1</resourceAmount>						<customResourceString>TSP_WeeblAmazingHorse_Lemonade_Deposit</customResourceString> <!-- I GUESS I'D BETTER NOT SHOW YOU WHERE THE LEMONADE IS MADE -->					</li>				</comps>			</value>		</match>		<!-- If the first XPATH does not exist, the original code stands. -->	</Operation>		<Operation Class="PatchOperationConditional">	<!-- Does this exist? -->		<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Equium"]</xpath>		<!-- If it does, do this replacement. -->		<match Class="PatchOperationAdd">			<success>Always</success>			<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName = "Alien_Equium" or defName = "Alien_PEquium" or defName = "Alien_KEquium" or defName = "Alien_PEquium" or defName = "Alien_VHDMaleEquium"]/comps</xpath>			<value>				<li Class="CompProperties_Spawner">					<thingToSpawn>ST_MikesLemonade</thingToSpawn>					<spawnCount>1</spawnCount>					<spawnIntervalRange>						<min>90000</min>						<max>130000</max>					</spawnIntervalRange>					<spawnMaxAdjacent>100</spawnMaxAdjacent>					<spawnForbidden>false</spawnForbidden>				</li>			</value>		</match>		<!-- If the first XPATH does not exist, the original code stands. -->	</Operation>			<Operation Class="PatchOperationConditional">	<!-- Does this exist? -->		<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Equium"]</xpath>		<!-- If it does, do this replacement. -->		<match Class="PatchOperationAdd">			<success>Always</success>			<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName = "Alien_Equium" or defName = "Alien_PEquium" or defName = "Alien_KEquium" or defName = "Alien_PEquium" or defName = "Alien_VHDMaleEquium"]/race</xpath>			<value>				<useMeatFrom>Horse</useMeatFrom>			</value>		</match>		<!-- If the first XPATH does not exist, the original code stands. -->	</Operation>			<!-- I THINK YOU'LL FIND THAT THE UNIVERSE PRETTY MUCH COVERS EVERYTHING -->	<!-- SHUT UP WOMAN, GET ON MY HORSE --></Patch>