<?xml version="1.0" encoding="utf-8" ?><Patch>	<!-- LOOK AT MY HORSE, MY HORSE IS AMAZING -->	<Operation Class="PatchOperationConditional">	<!-- Does this exist? -->		<xpath>/Defs/ThingDef[defName="Horse"]</xpath>		<!-- If it does, do this replacement. -->		<match Class="PatchOperationAdd">			<success>Always</success>			<xpath>/Defs/ThingDef[defName="Horse"]</xpath>			<value>				<comps>					<li Class="AnimalBehaviours.CompProperties_AnimalProduct">						<resourceDef>ST_MikesLemonade</resourceDef>						<gatheringIntervalDays>1.2</gatheringIntervalDays>						<resourceAmount>1</resourceAmount>						<customResourceString>TSP_WeeblAmazingHorse_Lemonade_Deposit</customResourceString> <!-- I GUESS I'D BETTER NOT SHOW YOU WHERE THE LEMONADE IS MADE -->					</li>				</comps>			</value>		</match>		<!-- If the first XPATH does not exist, the original code stands. -->	</Operation>		<!-- GET ON MY HORSE, I'LL TAKE YOU THROUGH THE UNIVERSE -->	<!-- AND ALL OF THE OTHER PLACES, TOO -->	<Operation Class="PatchOperationConditional">	<!-- Does this exist? -->		<xpath>/Defs/ThingDef[defName="MG_Horse"]</xpath>		<!-- If it does, do this replacement. -->		<match Class="PatchOperationAdd">			<success>Always</success>			<xpath>/Defs/ThingDef[defName="MG_Horse"]</xpath>			<value>				<comps>					<useMeatFrom>Horse</useMeatFrom>					<li Class="AnimalBehaviours.CompProperties_AnimalProduct">						<resourceDef>ST_MikesLemonade</resourceDef>						<gatheringIntervalDays>1.2</gatheringIntervalDays>						<resourceAmount>1</resourceAmount>						<customResourceString>TSP_WeeblAmazingHorse_Lemonade_Deposit</customResourceString> <!-- I GUESS I'D BETTER NOT SHOW YOU WHERE THE LEMONADE IS MADE -->					</li>				</comps>			</value>		</match>		<!-- If the first XPATH does not exist, the original code stands. -->	</Operation>		<!-- GET ON MY HORSE, I'LL TAKE YOU THROUGH THE UNIVERSE -->	<!-- AND ALL OF THE OTHER PLACES, TOO -->	<!-- I THINK YOU'LL FIND THAT THE UNIVERSE PRETTY MUCH COVERS EVERYTHING -->	<!-- SHUT UP WOMAN, GET ON MY HORSE --></Patch>